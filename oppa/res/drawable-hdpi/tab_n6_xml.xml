<selector xmlns:android="http://schemas.android.com/apk/res/android"> 
<!-- Non focused states -->

	<item android:state_focused="false" android:state_selected="false"	android:state_pressed="false" android:drawable="@drawable/tab_05" /> 
	<item android:state_focused="false" android:state_selected="true"	android:state_pressed="false" android:drawable="@drawable/tab_05_on" /> 
	<!-- Focused states -->
	<item android:state_focused="true" android:state_selected="false"	android:state_pressed="false" android:drawable="@drawable/tab_05_on"	/> 
	<item android:state_focused="true" android:state_selected="true"	android:state_pressed="false" android:drawable="@drawable/tab_05_on"	/> 
	<!-- Pressed -->
	<item android:state_pressed="true" android:drawable="@drawable/tab_05_on" /> 
</selector>