<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" android:background="#FFF5F5F5">
    <!-- 상단바 -->
    
    <LinearLayout
        android:id="@+id/toy_main_layout_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
       <ImageView
	        android:id="@+id/imageView1"
	        android:layout_width="480px"
	        android:layout_height="66px"
	        android:src="@drawable/main_title_bar" />
         <ImageView
             android:id="@+id/imageView2"
             android:layout_width="150px"
             android:layout_height="60px"
	        android:layout_marginTop="-61px"
             android:src="@drawable/n_5_title7" android:layout_gravity="center_horizontal"/>


         
         <Button
		    android:id="@+id/my_memo_delete"
		    android:layout_width="99px"
		    android:layout_height="30px"
		    android:layout_gravity="left"
		    android:layout_marginLeft="370px"
		    android:layout_marginTop="-41px"
             android:layout_marginBottom="10px"
		     android:background="@drawable/delete_btn1" 
		    />

	     
	    <LinearLayout
	        android:id="@+id/my_page_memo_tab"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal">
		    <ImageButton
		        android:id="@+id/my_page_memo_tab_btn_1"
		        android:layout_width="240px"
		        android:layout_height="48px"
		        android:background="@drawable/n_5_10on" android:layout_gravity="left"/>
		    <ImageButton
		        android:id="@+id/my_page_memo_tab_btn_2"
		        android:layout_width="240px"
		        android:layout_height="48px"
		        android:background="@drawable/n_5_11" android:layout_gravity="left"/>

	
	    </LinearLayout>
    </LinearLayout>
    
    <LinearLayout
	    android:id="@+id/home_main_layout_2"
	    android:layout_width="480px"
	    android:layout_height="74px"
	    android:background="@drawable/tab_n1"
	    android:orientation="horizontal" 
	    android:layout_gravity="bottom" 
	    android:layout_weight="1" 
	    android:layout_alignParentBottom="true"  >
	    <Button
	        android:id="@+id/mypage1_main_tab_btn_1"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n2_xml"
	        android:layout_gravity="left"/>
	    <Button
	        android:id="@+id/mypage1_main_tab_btn_2"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n3_xml"
	        android:layout_gravity="left"/>
	    
	    <Button
	        android:id="@+id/mypage1_main_tab_btn_3"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n4_xml"
	        android:layout_gravity="left"/>
	    
	    <Button
	        android:id="@+id/mypage1_main_tab_btn_4"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n5_xml"
	        android:layout_gravity="left"/>
	    
	    <ImageView
	        android:id="@+id/mypage1_main_tab_btn_5"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_05_on"
	        android:layout_gravity="left"/>
     </LinearLayout>
     <LinearLayout
	        android:id="@+id/toy_main_list_content_more"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:orientation="vertical" 
         	android:layout_above="@id/home_main_layout_2">
	    <Button
	        	android:id="@+id/myinfo_memo_listview_more_btn"
	            android:layout_width="480px"
	            android:layout_height="39px"
	          	android:layout_weight="1"
	           	android:background="@drawable/shop_list_view_more_btn"
	           	/>	
	</LinearLayout>

	<ListView
	        android:id="@+id/myinfo_memo_listview"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginTop="114px"
	     	android:background="#FFf5f5f5"
       		android:layout_above="@id/toy_main_list_content_more"  />    

</RelativeLayout>



