<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" android:background="#FFF5F5F5">
         <!-- 상단바 -->
     
    <LinearLayout
        android:id="@+id/layout_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
	    <ImageView
	        android:id="@+id/imageView1"
	        android:layout_width="480px"
	        android:layout_height="66px"
	        android:src="@drawable/main_title_bar" />
	    
	    <ImageView
	    	android:id="@+id/imageView2"
	        android:layout_width="150px"
	        android:layout_height="60px"
	        android:layout_marginLeft="10px"
	        android:layout_marginTop="-60px"
	        android:src="@drawable/comm_main_title" android:layout_gravity="center_horizontal"/>
	
	    <Button
	        android:id="@+id/commu_main_back"
	        android:layout_width="78px"
	        android:layout_height="30px"
	        android:layout_marginLeft="10px"
	        android:layout_marginTop="10px"
	        android:background="@drawable/common_back_btn"
	        android:layout_gravity="left"
	        android:visibility="gone"/>
	    <Button
	        android:id="@+id/bbs_main_notice"
	        android:layout_width="480px"
	        android:layout_height="86px"
	        android:background="@drawable/comm_main_1_btn"
	        android:layout_gravity="left"/>
	    <Button
	        android:id="@+id/bbs_main_epil"
	        android:layout_width="480px"
	        android:layout_height="86px"
	        android:background="@drawable/comm_main_2_btn"
	        android:layout_gravity="left"/>
	    <Button
	        android:id="@+id/bbs_main_free"
	        android:layout_width="480px"
	        android:layout_height="86px"
	        android:background="@drawable/comm_main_3_btn"
	        android:layout_gravity="left"/>
	    <Button
	        android:id="@+id/bbs_main_qna"
	        android:layout_width="480px"
	        android:layout_height="87px"
	        android:background="@drawable/comm_main_4_btn"
	        android:layout_gravity="left"/>
	    <Button
	        android:id="@+id/bbs_report"
	        android:layout_width="480px"
	        android:layout_height="87px"
	        android:background="@drawable/comm_main_5_btn"
	        android:layout_gravity="left"/>       
	</LinearLayout>
    <LinearLayout
	    android:id="@+id/layout_2"
	    android:layout_width="480px"
	    android:layout_height="74px"
	    android:layout_gravity="bottom"
	    android:background="@drawable/tab_n1"
	    android:orientation="horizontal"
	    android:layout_alignParentBottom="true" >

	    <Button
	        android:id="@+id/commu_main_tab_btn_1"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n2_xml"
	        android:layout_gravity="left"/>
	    <Button
	        android:id="@+id/commu_main_tab_btn_2"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n3_xml"
	        android:layout_gravity="left"/>
	    
	    <Button
	        android:id="@+id/commu_main_tab_btn_3"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n4_xml"
	        android:layout_gravity="left"/>
	    
	    <ImageView
	        android:id="@+id/commu_main_tab_btn_4"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_04_on"
	        android:layout_gravity="left"/>
	    
	    <Button
	        android:id="@+id/commu_main_tab_btn_5"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n6_xml"
	        android:layout_gravity="left"/>
     </LinearLayout> 
     
    
         
    <FrameLayout
        android:id="@+id/layout_2"
        android:layout_width="480px"
        android:layout_height="800px"
        android:orientation="vertical">
	    <TextView
        	android:id="@+id/commu_new1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="175px"
            android:layout_marginTop="105px"
            android:text="new"
            android:textColor="#FFFF3232"
            android:textSize="15px" 
            android:layout_gravity="left"/>
	    <TextView
        	android:id="@+id/commu_new2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="300px"
            android:layout_marginTop="190px"
            android:text="new"
            android:textColor="#FFFF3232"
            android:textSize="15px" 
            android:layout_gravity="left"/>  
       <TextView
        	android:id="@+id/commu_new3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="210px"
            android:layout_marginTop="275px"
            android:text="new"
            android:textColor="#FFFF3232"
            android:textSize="15px" 
            android:layout_gravity="left"/>
       <TextView
        	android:id="@+id/commu_new4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="130px"
            android:layout_marginTop="360px"
            android:text="new"
            android:textColor="#FFFF3232"
            android:textSize="15px" 
            android:layout_gravity="left"/>
	</FrameLayout>
	   
</RelativeLayout>