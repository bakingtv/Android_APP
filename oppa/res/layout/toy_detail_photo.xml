<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" android:background="#FFF5F5F5">
    <!-- 상단바 -->
    
    <LinearLayout
        android:id="@+id/toy_main_layout_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >
	    <ImageView
	        android:id="@+id/imageView1"
	        android:layout_width="480px"
	        android:layout_height="66px"
	        android:src="@drawable/main_title_bar" />
	    <ImageView
	    	android:id="@+id/imageView2"
	        android:layout_width="180px"
	        android:layout_height="60px"
	        android:layout_marginTop="-60px"
	        android:src="@drawable/n_3_title2" android:layout_gravity="center_horizontal"/>
	        <Button
        android:id="@+id/toy_detail_title_bar_zzim"
        android:layout_width="90px"
        android:layout_height="30px"
        android:layout_marginLeft="375px"
        android:layout_marginTop="-45px"
        android:layout_marginBottom="10px"
        android:background="@drawable/common_zzim_btn" android:layout_gravity="left"/>
	    
		<LinearLayout
	        android:id="@+id/bbs_main_list_header"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal"
	        >
		         
		    <ImageView
		        android:id="@+id/toy_bbs_image_main"
		        android:layout_width="152px"
		        android:layout_height="116px"
		        android:layout_marginLeft="12px"
		        android:layout_marginTop="12px"
		        android:layout_marginBottom="45px"
		        android:src="@drawable/shop_list_view_temp_image" />	
	        
		    <LinearLayout
		        android:id="@+id/bbs_main_list_header2"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:orientation="vertical">
			    			    	<LinearLayout
					    android:id="@+id/bbs_main_list_header_2"
					    android:layout_width="match_parent"
					    android:layout_height="wrap_content"
					    android:orientation="horizontal">
					    <TextView
					        android:id="@+id/toy_bbs_name"
					        android:layout_width="wrap_content"
					        android:layout_height="wrap_content"
					        android:layout_marginLeft="12px"
					        android:layout_marginTop="10px"
					        android:text="이름 : "
					        android:textColor="#FF323232"
					        android:textSize="15px" />
					    <LinearLayout
						    android:id="@+id/bbs_main_list_header_2"
						    android:layout_width="match_parent"
						    android:layout_height="wrap_content"
						    android:orientation="horizontal"
						    android:gravity="right">
							<Button
						    	android:id="@+id/toy_bbs_call_btn"
						        android:layout_width="69px"
						        android:layout_height="33px"
								android:layout_marginRight="12px"
								android:layout_marginTop="10px"
						        android:background="@drawable/shop_detail_call_btn"
						        android:gravity="right"/>
						</LinearLayout>
					</LinearLayout>


			    <TextView
			        android:id="@+id/toy_bbs_shop_name"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="12px"
			        android:layout_marginTop="10px"
			        android:text="소속명 : "
			        android:textColor="#FF323232"
			        android:textSize="15px" />		        
	
		  
			<LinearLayout
				android:id="@+id/bbs_main_list_header_1"
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:orientation="horizontal">
				<TextView
				        android:id="@+id/toy_bbs_tel"
				        android:layout_width="wrap_content"
				        android:layout_height="wrap_content"
				        android:layout_marginLeft="12px"
				        android:layout_marginTop="3px"
				        android:text="전화 : 010-2343-3921"
				        android:textColor="#FF323232"
				        android:textSize="15px" />
			 </LinearLayout>
		     <LinearLayout
		        android:id="@+id/bbs_main_list_header_3"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:orientation="horizontal">

				<TextView
			        android:id="@+id/toy_bbs_addr"
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="12px"
			        android:layout_marginTop="3px"
			        android:text="주소 :  서울시 강남구 논현동 \n 가로수 2길 동화빌딩 지하1층"
			        android:textColor="#FF323232"
			        android:textSize="15px" />
				</LinearLayout>
				<TextView
					android:id="@+id/toy_bbs_score"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="left"
					android:layout_marginLeft="12px"
					android:layout_marginTop="3px"
					android:text="찜횟수 :"
					android:textColor="#FF323232"
					android:textSize="15px" />
			</LinearLayout>
		</LinearLayout>
		        
	   	<LinearLayout
	        android:id="@+id/toy_detail_tab"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal">
		    <Button
		        android:id="@+id/toy_detail_tab_btn_1"
		        android:layout_width="246px"
		        android:layout_height="48px"
		        android:background="@drawable/toy_detail_story" android:layout_gravity="left"/>
		    <Button
		        android:id="@+id/toy_detail_tab_btn_2"
		        android:layout_width="234px"
		        android:layout_height="48px"
		        android:background="@drawable/toy_detail_picon" android:layout_gravity="left"/>
	    </LinearLayout>
    </LinearLayout>
    <LinearLayout
		android:id="@+id/toy_main_layout_2"
		android:layout_width="480px"
		android:layout_height="74px"
		android:background="@drawable/tab_n1"
		android:orientation="vertical" 
		android:layout_gravity="bottom" 
		android:layout_weight="1" 
		android:layout_alignParentBottom="true"  >
        <LinearLayout
			android:id="@+id/toy_main_layout_4"
			android:layout_width="480px"
			android:layout_height="74px"
			android:orientation="horizontal"   >
			<Button
				android:id="@+id/toy_main_tab_btn_1"
				android:layout_width="96px"
				android:layout_height="74px"
				android:background="@drawable/tab_n2_xml"
				android:layout_gravity="left"/>
			<Button
				android:id="@+id/toy_main_tab_btn_2"
				android:layout_width="96px"
				android:layout_height="74px"
				android:background="@drawable/tab_n3_xml"
				android:layout_gravity="left"/>
			
			<ImageView
				android:id="@+id/toy_main_tab_btn_3"
				android:layout_width="96px"
				android:layout_height="74px"
				android:background="@drawable/tab_03_on"
				android:layout_gravity="left"/>
					
			<Button
				android:id="@+id/toy_main_tab_btn_4"
				android:layout_width="96px"
				android:layout_height="74px"
				android:background="@drawable/tab_n5_xml"
				android:layout_gravity="left"/>
					
			<Button
				android:id="@+id/toy_main_tab_btn_5"
				android:layout_width="96px"
				android:layout_height="74px"
				android:background="@drawable/tab_n6_xml"
				android:layout_gravity="left"/>
            
        </LinearLayout>
        
        

	 </LinearLayout>

    
    
    <FrameLayout
	        android:id="@+id/toy_main_list_content_more"
	        android:layout_width="480px"
	        android:layout_height="400px"
	        android:orientation="vertical" 
         	android:layout_above="@id/toy_main_layout_2">
		    <ImageView
		        android:id="@+id/toy_detail_photo1"
		        android:layout_width="144px"
		        android:layout_height="144px"
		        android:layout_gravity="left"
        		android:layout_marginLeft="12px"
        		android:layout_marginTop="12px"
		        android:src="@drawable/toy_detail_sample_photo" />
		    
		    <ImageView
		        android:id="@+id/toy_detail_photo2"
		        android:layout_width="144px"
		        android:layout_height="144px"
		        android:layout_gravity="left"
        		android:layout_marginLeft="168px"
        		android:layout_marginTop="12px"
		        android:src="@drawable/toy_detail_sample_photo" />
		    		    
		   <ImageView
		        android:id="@+id/toy_detail_photo3"
		        android:layout_width="144px"
		        android:layout_height="144px"
		        android:layout_gravity="left"
        		android:layout_marginLeft="322px"
        		android:layout_marginTop="12px"
		        android:src="@drawable/toy_detail_sample_photo" />
		   
		   <ImageView
		        android:id="@+id/toy_detail_photo4"
		        android:layout_width="144px"
		        android:layout_height="144px"
		        android:layout_gravity="left"
        		android:layout_marginLeft="12px"
        		android:layout_marginTop="168px"
		        android:src="@drawable/toy_detail_sample_photo" />
		   
		   <ImageView
		        android:id="@+id/toy_detail_photo5"
		        android:layout_width="144px"
		        android:layout_height="144px"
		        android:layout_gravity="left"
        		android:layout_marginLeft="168px"
        		android:layout_marginTop="168px"
		        android:src="@drawable/toy_detail_sample_photo" />
		   
		   
		   <ImageView
		        android:id="@+id/toy_detail_photo6"
		        android:layout_width="144px"
		        android:layout_height="144px"
		        android:layout_gravity="left"
        		android:layout_marginLeft="322px"
        		android:layout_marginTop="168px"
		        android:src="@drawable/toy_detail_sample_photo" />
	</FrameLayout>
</RelativeLayout>