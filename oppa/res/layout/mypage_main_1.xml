<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" android:background="#FFF5F5F5">
         <!-- 상단바 -->
         
    <LinearLayout
        android:id="@+id/layout_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
	    <ImageView
	        android:id="@+id/imageView1"
	        android:layout_width="480px"
	        android:layout_height="66px"
	        android:src="@drawable/main_title_bar" />
	    
	    <ImageView
	    	android:id="@+id/imageView2"
	        android:layout_width="150px"
	        android:layout_height="60px"
	        android:layout_marginTop="-60px"
	        android:src="@drawable/mypage_main_title" android:layout_gravity="center_horizontal"/>
	
	    <Button
	        android:id="@+id/commu_main_back"
	        android:layout_width="78px"
	        android:layout_height="30px"
	        android:layout_marginLeft="10px"
	        android:layout_marginTop="-30px"
	        android:background="@drawable/common_back_btn"
	        android:layout_gravity="left"
	        android:visibility="gone"/>
	    <Button
	        android:id="@+id/mypage_main_memo_btn"
	        android:layout_width="480px"
	        android:layout_height="78px"
	        android:background="@drawable/mypage_main_memo_btn"
	        android:layout_gravity="left"/>
	    <Button
	        android:id="@+id/myinfo_shop_1"
	        android:layout_width="480px"
	        android:layout_height="78px"
	        android:background="@drawable/mypage_main_shop_btn"
	        android:layout_gravity="left"/>
	    <Button
	        android:id="@+id/myinfo_sister_1"
	        android:layout_width="480px"
	        android:layout_height="78px"
	        android:background="@drawable/mypage_main_sister_btn"
	        android:layout_gravity="left"/>
	    <Button
	        android:id="@+id/my_main_grade"
	        android:layout_width="480px"
	        android:layout_height="78px"
	        android:background="@drawable/mypage_main_grade_btn"
	        android:layout_gravity="left"/>
	    <Button
	        android:id="@+id/my_myinfo_rewrite123"
	        android:layout_width="480px"
	        android:layout_height="75px"
	        android:background="@drawable/mypage_main_info_btn"
	        android:layout_gravity="left"/>  
	        

	        
	        
	    
	   <ImageView
	        android:id="@+id/imageview10"
	        android:layout_width="480px"
	        android:layout_height="77px"
	        android:src="@drawable/mypage_main_autologin"
	        android:layout_gravity="left"/> 
	        
	      <RadioGroup
	       android:id="@+id/my_autologin"
	       android:layout_width="match_parent"
	       android:layout_height="wrap_content"
	       android:layout_gravity="left"
	       android:layout_marginLeft="250px"
       		android:layout_marginTop="-77px"
	       android:orientation="horizontal">
	       <RadioButton
	           android:id="@+id/autologin1"
	           android:layout_width="wrap_content"
	           android:layout_height="wrap_content"
	           android:layout_gravity="left"
	           android:checked="true"
	           android:text="On     "
	           android:textColor="#FF323232" />
	
	       <RadioButton
	           android:id="@+id/autologin"
	           android:layout_width="wrap_content"
	           android:layout_height="wrap_content"
	           android:layout_gravity="left"
	           android:text="Off"
	           android:textColor="#FF323232" />
	   </RadioGroup>
	   
	   <Button
	        android:id="@+id/my_memo_send_btn"
	        android:layout_width="480px"
	        android:layout_height="79px"
	        android:layout_marginTop="10px"
	        android:background="@drawable/mypage_main_admin_btn"
	        android:layout_gravity="left"/>  
	</LinearLayout>
	
    	<LinearLayout
	    android:id="@+id/home_main_layout_2"
	    android:layout_width="480px"
	    android:layout_height="74px"
	    android:background="@drawable/tab_n1"
	    android:orientation="horizontal" 
	    android:layout_gravity="bottom" 
	    android:layout_weight="1" 
	    android:layout_alignParentBottom="true"  >
	    <Button
	        android:id="@+id/mypage1_main_tab_btn_1"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n2_xml"
	        android:layout_gravity="left"/>
	    <Button
	        android:id="@+id/mypage1_main_tab_btn_2"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n3_xml"
	        android:layout_gravity="left"/>
	    
	    <Button
	        android:id="@+id/mypage1_main_tab_btn_3"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n4_xml"
	        android:layout_gravity="left"/>
	    
	    <Button
	        android:id="@+id/mypage1_main_tab_btn_4"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n5_xml"
	        android:layout_gravity="left"/>
	    
	    <ImageView
	        android:id="@+id/mypage1_main_tab_btn_5"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_05_on"
	        android:layout_gravity="left"/>
     </LinearLayout>
   
</RelativeLayout>