<?xml version="1.0" encoding="utf-8"?>
<FrameLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#FFF5F5F5" >
	<!-- 상단바 -->
    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="480px"
        android:layout_height="66px"
        android:src="@drawable/main_title_bar" />
    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="210px"
        android:layout_height="60px"
        android:src="@drawable/home_login_id_find_title"
        android:layout_gravity="center_horizontal" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="480px"
        android:layout_height="47px"
        android:src="@drawable/home_login_id_find_title1" android:layout_gravity="left" android:layout_marginTop="66px"/>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="480px"
        android:layout_height="47px"
        android:src="@drawable/home_login_id_find_title2" android:layout_marginTop="405px" android:layout_gravity="left"/>

    <Button
        android:id="@+id/home_login_id_find_btn"
        android:layout_width="186px"
        android:layout_height="57px"
        android:layout_gravity="left" android:layout_marginLeft="163px" android:layout_marginTop="310px" android:background="@drawable/home_login_id_find_ok_btn"/>

    <EditText
        android:id="@+id/home_find_nick"
        android:layout_width="180dp"
        android:layout_height="30dp"
        android:layout_gravity="left"
        android:layout_marginLeft="163px"
        android:layout_marginTop="150px"
         >

    </EditText>

    <Spinner
        android:id="@+id/home_id_finder_spinner"
        android:layout_width="180dp"
        android:layout_height="34dp"
        android:layout_gravity="left"
        android:layout_marginLeft="163px"
        android:layout_marginTop="195px" />

    <EditText
        android:id="@+id/home_find_result"
        android:layout_width="180dp"
        android:layout_height="30dp"
        android:layout_marginLeft="163px"
        android:layout_marginTop="250px" android:layout_gravity="left"/>

    <EditText
        android:id="@+id/home_find_id"
        android:layout_width="180dp"
        android:layout_height="30dp"
        android:layout_marginLeft="163px"
        android:layout_marginTop="500px" 
         android:editable="false"
        android:layout_gravity="left" android:focusable="false"/>

    <EditText
        android:id="@+id/home_find_pass"
        android:layout_width="180dp"
        android:layout_height="30dp"
        android:layout_marginLeft="163px"
        android:layout_marginTop="550px" 
        android:editable="false"
        android:layout_gravity="left" android:focusable="false"/>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="닉네임"
        android:textAppearance="?android:attr/textAppearanceLarge" android:layout_gravity="left" android:layout_marginTop="160px" android:layout_marginLeft="20px"/>

    <TextView
        android:id="@+id/TextView01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20px"
        android:layout_marginTop="210px"
        android:text="비밀번호질문"
        android:textAppearance="?android:attr/textAppearanceLarge" android:layout_gravity="left"/>

    <TextView
        android:id="@+id/TextView02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20px"
        android:layout_marginTop="260px"
        android:text="비밀번호답"
        android:textAppearance="?android:attr/textAppearanceLarge" android:layout_gravity="left"/>

    <TextView
        android:id="@+id/TextView03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20px"
        android:layout_marginTop="510px"
        android:text="ID"
        android:textAppearance="?android:attr/textAppearanceLarge" android:layout_gravity="left"/>

    <TextView
        android:id="@+id/TextView04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20px"
        android:layout_marginTop="560px"
        android:text="비밀번호"
        android:textAppearance="?android:attr/textAppearanceLarge" android:layout_gravity="left"/>

    <Button
        android:id="@+id/home_login_id_find_back"
        android:layout_width="78px"
        android:layout_height="30px"
        android:layout_gravity="left" android:layout_marginTop="15px" android:layout_marginLeft="10px" android:background="@drawable/common_back_btn"/>

</FrameLayout >