<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" android:background="#FFF5F5F5">
         <!-- 상단바 -->
         
    <LinearLayout
        android:id="@+id/layout_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
	    <LinearLayout
	        android:id="@+id/bbs_main_list_titlebar"
	        android:layout_width="match_parent"
	        android:layout_height="65px"
	        android:orientation="vertical">
		    <!-- 상단바 -->
		    <ImageView
		    	android:id="@+id/imageView1"
		        android:layout_width="480px"
		        android:layout_height="66px"
		        android:src="@drawable/main_title_bar" />
			<ImageView
			 	android:id="@+id/mypage_shop_main_list_title"
			    android:layout_width="180px"
			    android:layout_height="60px"
			    android:layout_marginTop="-66px"
			    android:src="@drawable/n_5_title5" android:layout_gravity="center_horizontal"/>
			<Button
				android:id="@+id/bbs_main_list_find_back"
			    android:layout_width="78px"
			    android:layout_height="30px"
			    android:layout_gravity="left"
			    android:layout_marginTop="-45px" 
			    android:layout_marginLeft="10px" 
			    android:background="@drawable/common_back_btn"
			    android:visibility="gone"/>
			
	
	
			<Button
			    android:id="@+id/my_shop_delete"
			    android:layout_width="99px"
			    android:layout_height="30px"
			    android:layout_gravity="left"
			    android:layout_marginLeft="370px"
			    android:layout_marginTop="-55px"
			    android:background="@drawable/delete_btn1"
			    />
			
		 </LinearLayout>
	
	
	</LinearLayout>
	
    <LinearLayout
	    android:id="@+id/home_main_layout_2"
	    android:layout_width="480px"
	    android:layout_height="74px"
	    android:background="@drawable/tab_n1"
	    android:orientation="horizontal" 
	    android:layout_gravity="bottom" 
	    android:layout_weight="1" 
	    android:layout_alignParentBottom="true"  >
	    <Button
	        android:id="@+id/mypage1_main_tab_btn_1"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n2_xml"
	        android:layout_gravity="left"/>
	    <Button
	        android:id="@+id/mypage1_main_tab_btn_2"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n3_xml"
	        android:layout_gravity="left"/>
	    
	    <Button
	        android:id="@+id/mypage1_main_tab_btn_3"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n4_xml"
	        android:layout_gravity="left"/>
	    
	    <Button
	        android:id="@+id/mypage1_main_tab_btn_4"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n5_xml"
	        android:layout_gravity="left"/>
	    
	    <ImageView
	        android:id="@+id/mypage1_main_tab_btn_5"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_05_on"
	        android:layout_gravity="left"/>
     </LinearLayout>
     <LinearLayout
	        android:id="@+id/shop_main2_list_content_more"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:orientation="vertical" 
         	android:layout_above="@id/home_main_layout_2">
	    <Button
	        	android:id="@+id/bbs_main_list_more"
	            android:layout_width="480px"
	            android:layout_height="39px"
	          	android:layout_weight="1"
	           	android:background="@drawable/shop_list_view_more_btn"
	           	/>	
	</LinearLayout>
	<ListView
	        android:id="@+id/myinfo_main_list_view"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginTop="66px"
	     	android:background="#FFf5f5f5"
       		android:layout_above="@id/shop_main2_list_content_more"  />  
</RelativeLayout>






