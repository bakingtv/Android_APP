<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" android:background="#FFF5F5F5">
         <!-- 상단바 -->
     
    <FrameLayout
        android:id="@+id/layout_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
     <!-- 상단바 -->
    <ImageView
        android:id="@+id/shop_qna_title"
        android:layout_width="480px"
        android:layout_height="66px"
        android:src="@drawable/main_title_bar" />


     <!-- 상단바 -->
    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="match_parent"
        android:layout_height="66px"
        android:src="@drawable/main_title_bar" />

         <ImageView
             android:id="@+id/imageView2"
             android:layout_width="150px"
             android:layout_height="60px"
             android:src="@drawable/shop_detail_epilogue_title"
              android:layout_gravity="center_horizontal"/>
         

         <EditText
             android:id="@+id/bbs_wr_content"
             android:layout_width="match_parent"
             android:layout_height="334dp"
             android:layout_gravity="left"
             android:layout_marginTop="80px"
             android:gravity="top"
             android:inputType="textMultiLine" >
         </EditText>
         
         <Button
	       android:id="@+id/bbs_wr_score"
	       android:layout_width="186px"
	       android:layout_height="57px"
	       android:layout_gravity="left" android:layout_marginLeft="280px" android:layout_marginTop="580px" android:background="@drawable/shop_grade_btn"/>
  	</FrameLayout>
    <LinearLayout
	    android:id="@+id/layout_2"
	    android:layout_width="480px"
	    android:layout_height="74px"
	    android:layout_gravity="bottom"
	    android:background="@drawable/tab_n1"
	    android:orientation="horizontal"
	    android:layout_alignParentBottom="true" >

	    <Button
	        android:id="@+id/commu_main_tab_btn_1"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n2_xml"
	        android:layout_gravity="left"/>
	    <Button
	        android:id="@+id/commu_main_tab_btn_2"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n3_xml"
	        android:layout_gravity="left"/>
	    
	    <Button
	        android:id="@+id/commu_main_tab_btn_3"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n4_xml"
	        android:layout_gravity="left"/>
	    
	    <ImageView
	        android:id="@+id/commu_main_tab_btn_4"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_04_on"
	        android:layout_gravity="left"/>
	    
	    <Button
	        android:id="@+id/commu_main_tab_btn_5"
	        android:layout_width="96px"
	        android:layout_height="74px"
	        android:background="@drawable/tab_n6_xml"
	        android:layout_gravity="left"/>
     </LinearLayout>  
</RelativeLayout>
