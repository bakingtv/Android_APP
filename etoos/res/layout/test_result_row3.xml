<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="92px"
    android:layout_weight="10"
    android:orientation="vertical" 
    android:background="#FFFFFF">

    <FrameLayout
        android:id="@+id/mainlist_box"
        android:layout_width="match_parent"
        android:layout_height="92px"
        android:background="@drawable/list_box"
        android:orientation="horizontal"
        android:weightSum="1" >

        <TextView
            android:id="@+id/number"
            android:layout_width="67px"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_weight="0.2"
            android:background="#eecccc"
            android:gravity="center"
            android:text="37"
            android:textSize="24sp" />

        <FrameLayout
            android:id="@+id/shop_intro_1_1"
            android:layout_width="480px"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/number_1"
                android:layout_width="55px"
                android:layout_height="65px"
                android:layout_marginBottom="12px"
                android:layout_marginLeft="77px"
                android:layout_marginTop="12px"
                android:src="@drawable/basic_0" />

            <ImageView
                android:id="@+id/number_2"
                android:layout_width="55px"
                android:layout_height="65px"
                android:layout_marginBottom="12px"
                android:layout_marginLeft="132px"
                android:layout_marginTop="12px"
                android:src="@drawable/basic_0" />

            <ImageView
                android:id="@+id/number_3"
                android:layout_width="55px"
                android:layout_height="65px"
                android:layout_marginBottom="12px"
                android:layout_marginLeft="187px"
                android:layout_marginTop="12px"
                android:src="@drawable/basic_0" />

            <ImageView
                android:id="@+id/number_4"
                android:layout_width="55px"
                android:layout_height="65px"
                android:layout_marginBottom="12px"
                android:layout_marginLeft="242px"
                android:layout_marginTop="12px"
                android:src="@drawable/basic_4"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/number_5"
                android:layout_width="94px"
                android:layout_height="69px"
                android:layout_marginBottom="10px"
                android:layout_marginLeft="247px"
                android:layout_marginTop="10px"
                android:src="@drawable/x_box" />

            <ImageView
                android:id="@+id/sol_minus"
                android:layout_width="50px"
                android:layout_height="62px"
                android:layout_marginLeft="348px"
                android:layout_marginTop="17px"
                android:layout_weight="0"
                android:background="@drawable/baejeom_left" />

            <ImageView
                android:id="@+id/number_x"
                android:layout_width="47px"
                android:layout_height="62px"
                android:layout_marginLeft="388px"
                android:layout_marginTop="17px"
                android:layout_weight="0"
                android:background="@drawable/baejeom_box" />

            <TextView
                android:id="@+id/result_number1111"
                android:layout_width="50px"
                android:layout_height="62px"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="385px"
                android:layout_weight="0.2"
                android:gravity="center"
                android:text="37"
                android:textColor="#FFFFFF"
                android:textSize="30px" />

            <ImageView
                android:id="@+id/sol_plus"
                android:layout_width="50px"
                android:layout_height="62px"
                android:layout_marginLeft="425px"
                android:layout_marginTop="17px"
                android:layout_weight="0"
                android:background="@drawable/baejeom_right" />
        </FrameLayout>

        <ImageView
            android:id="@+id/number_x"
            android:layout_width="57px"
            android:layout_height="49px"
            android:layout_marginBottom="12px"
            android:layout_marginLeft="408px"
            android:layout_marginTop="22px"
            android:layout_weight="0"
            android:background="@drawable/q_type"
            android:visibility="gone" />
    </FrameLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="2sp"
        android:layout_marginTop="-2sp"
        android:src="@drawable/line" />

</LinearLayout>