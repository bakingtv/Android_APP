		    android:layout_width="match_parent"
		    android:layout_height="68px"
		    android:orientation="vertical"
		    android:background="@drawable/title_box" >
		    
            <ImageView
                android:id="@+id/title_icon"
                android:layout_width="53px"
                android:layout_height="49px"
                android:layout_gravity="left"
                android:layout_marginLeft="12px"
                android:layout_marginTop="9px"
                android:background="@drawable/back_btn" />

            
            <TextView
		      	android:id="@+id/title_name"
		       	android:layout_width="wrap_content"
		       	android:layout_height="wrap_content"
		       	android:ellipsize="end"
		       	android:lines="1"
		       	android:maxLines="1"
		       	android:layout_gravity="center"
		       	android:text="과목선택"
		       	android:textColor="#FFFFFF"
		    	android:textSize="25px" />
            
             <TextView
		      	android:id="@+id/title_desc"
		       	android:layout_width="wrap_content"
		       	android:layout_height="wrap_content"
		       	android:layout_marginLeft="98px"
		       	android:layout_marginRight="10px"
                android:layout_marginTop="10px"
		       	android:ellipsize="end"
		       	android:lines="1"
		       	android:maxLines="1"
		       	android:text=""
		       	android:textColor="#FFFFFF"
		    	android:textSize="0dip" />
             
             <ImageView
                android:id="@+id/title_icon2"
                android:layout_width="53px"
                android:layout_height="49px"
                android:layout_gravity="left"
                android:layout_marginLeft="416px"
                android:layout_marginTop="9px"
                android:background="@drawable/itemwritersmode_btn" />
             

	    </FrameLayout>
	    
         
