<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="40px"
    android:orientation="vertical"
    android:background="#FFFFFF" >

    <FrameLayout
        android:id="@+id/result_data_4"
        android:layout_width="450px"
        android:layout_height="40px"
        android:layout_marginLeft="14px"
        android:background="#F6F6F6"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/sol_1"
            android:layout_width="26px"
            android:layout_height="26px"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="10px"
            android:background="@drawable/white_0"
            android:gravity="left"
            android:textColor="#3E3E3E" />

        <TextView
            android:id="@+id/sol_time_1"
            android:layout_width="70px"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="50px"
            android:layout_marginTop="0px"
            android:text="11:11"
            android:textColor="#3E3E3E"
            android:textSize="20px" />
        
        <ImageView
            android:id="@+id/sol_2"
            android:layout_width="26px"
            android:layout_height="26px"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="130px"
            android:background="@drawable/white_0"
            android:gravity="left"
            android:textColor="#3E3E3E" />

        <TextView
            android:id="@+id/sol_time_2"
            android:layout_width="70px"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="170px"
            android:layout_marginTop="0px"
            android:text="11:11"
            android:textColor="#3E3E3E"
            android:textSize="20px" />
        
        
        <ImageView
            android:id="@+id/sol_3"
            android:layout_width="26px"
            android:layout_height="26px"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="230px"
            android:background="@drawable/white_0"
            android:gravity="left"
            android:textColor="#3E3E3E" />

        <TextView
            android:id="@+id/sol_time_3"
            android:layout_width="70px"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="270px"
            android:layout_marginTop="0px"
            android:text="11:11"
            android:textColor="#3E3E3E"
            android:textSize="20px" />
        
        
        <ImageView
            android:id="@+id/sol_4"
            android:layout_width="26px"
            android:layout_height="26px"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="340px"
            android:background="@drawable/white_0"
            android:gravity="left"
            android:textColor="#3E3E3E" />

        <TextView
            android:id="@+id/sol_time_4"
            android:layout_width="70px"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="380px"
            android:layout_marginTop="0px"
            android:text="11:11"
            android:textColor="#3E3E3E"
            android:textSize="20px" />
        
        
    </FrameLayout>

</LinearLayout>