<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="match_parent" android:orientation="vertical">


     <WebView
         android:id="@+id/webView"
         android:layout_width="match_parent"
         android:layout_height="727px"
         android:layout_gravity="left"
         android:keepScreenOn="true" >
	</WebView>
	
    <LinearLayout
        android:id="@+id/bottom_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_gravity="bottom"
        android:gravity="center"
        android:orientation="vertical" >

		<LinearLayout
			android:id="@+id/bottom_menu_1"
			android:layout_width="480px"
			android:layout_height="74px"
			android:orientation="horizontal"   >
			<ImageView
					android:id="@+id/bottom_1"
					android:layout_width="120px"
					android:layout_height="74px"
					android:background="@drawable/footer_off_01"
					android:layout_gravity="left"/>
			<ImageView
					android:id="@+id/bottom_2"
					android:layout_width="120px"
					android:layout_height="74px"
					android:background="@drawable/footer_off_02"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_3"
					android:layout_width="120px"
					android:layout_height="74px"
					android:background="@drawable/footer_off_03"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_4"
					android:layout_width="120px"
					android:layout_height="74px"
					android:background="@drawable/footer_off_03"
					android:layout_gravity="left"/>
			
		</LinearLayout>
	 </LinearLayout>
</FrameLayout>
