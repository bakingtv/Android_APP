<?xml version="1.0" encoding="utf-8"?>
<LinearLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
     android:background="#E2E4E6"
    android:orientation="vertical"
    android:layout_weight="1"
    android:gravity="center">
    
    <LinearLayout
	   	android:id="@+id/main_row_0"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical"
	    android:gravity="right">
	    

		    <ImageView
		        android:id="@+id/main_row_0_image"
		        android:layout_width="44px"
		        android:layout_height="44px"
		        android:layout_marginTop="8px"
		        android:layout_marginRight="20px"
		        android:background="@drawable/traveltetailsetting"
		        />
		    
	</LinearLayout >
    
    <LinearLayout
	   	android:id="@+id/main_row_1"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical"
	    android:gravity="center">
	    

		    <ImageView
		        android:id="@+id/main_row_1_image"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_marginTop="8px"
		        />
		    
	</LinearLayout >
	
    <LinearLayout
	   	android:id="@+id/main_row_2"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical"
	    android:layout_marginTop="5dip">
	    
        <ImageView
		        android:id="@+id/main_row_2_back"
		        android:layout_width="match_parent"
	    		android:layout_height="26px"
	    		android:background="@drawable/text_1"/>
        
        <LinearLayout
		   	android:id="@+id/main_row_2_2"
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:orientation="vertical"
		    android:textColor="#FF323232"
		    android:layout_marginTop="-20px"
		    android:background="@drawable/text_2">
       
        <TextView
		     android:id="@+id/main_row_2_title"
		     android:layout_width="match_parent"
		     android:layout_height="wrap_content"
		     android:layout_marginLeft="20px"
		     android:layout_marginRight="20px"
		     android:text="테스트1111"
		     android:textColor="#FF323232"
		     android:textSize="13dip" />
		
	    </LinearLayout >
	   	<ImageView
			android:id="@+id/main_row_2_back_2"
		    android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
	    	android:background="@drawable/text_3"/>
	</LinearLayout >
	
     <LinearLayout
	   	android:id="@+id/main_row_3"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical">
	    <ImageView
		   android:id="@+id/main_row_3_line"
		   android:layout_width="match_parent"
		   android:layout_height="wrap_content"
		   android:layout_marginTop="5dip"	 
		   android:background="@drawable/dot_line"/>

	    <LinearLayout
	        android:id="@+id/main_row_3"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal"
	        android:layout_marginTop="5dip"	
	        android:gravity="right" >
        	<ImageView
		        android:id="@+id/main_row_3_btn_1"
		        android:layout_width="100px"
		        android:layout_height="44px"
		        android:layout_marginRight="10px"	        
		        android:background="@drawable/re_1"
		        />
	        <ImageView
		        android:id="@+id/main_row_3_btn_2"
		        android:layout_width="100px"
		        android:layout_height="44px"
		        android:layout_marginRight="10px"	        
		        android:background="@drawable/delete_1"
		        />
	        <ImageView
		        android:id="@+id/main_row_3_btn_3"
		        android:layout_width="100px"
		        android:layout_height="44px"
		        android:layout_marginRight="10px"	        
		        android:background="@drawable/sns_1_bt"
		        />

        </LinearLayout >
	</LinearLayout >
	
    <LinearLayout
	   	android:id="@+id/main_row_4"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical">
	    <ImageView
		   android:id="@+id/main_row_4_line"
		   android:layout_width="match_parent"
		   android:layout_height="wrap_content"
		   android:layout_marginTop="5dip"	
		   android:background="@drawable/dot_line"/>
	    <LinearLayout
        	android:id="@+id/main_row_4_detail"
        	android:layout_width="match_parent"
        	android:layout_height="wrap_content"
        	android:orientation="vertical" >
		
	    	<LinearLayout
	        	android:id="@+id/main_row_4_detail_111"
	        	android:layout_width="match_parent"
	        	android:layout_height="wrap_content"
	       		android:orientation="horizontal"
	        	android:gravity="left"
	         	>


			<LinearLayout
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:gravity="left"
			    android:orientation="horizontal"
			    android:layout_alignParentTop="true"
			    android:layout_weight="1" >
        
		        <TextView
		            android:id="@+id/main_row_4_detail_nick"
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:layout_marginLeft="5px"
		            android:layout_marginTop="5px"
		            android:ellipsize="end"
		            android:gravity="left"
		            android:text="Nick"
		            android:textColor="#000000"
		            android:textSize="15dip"
		            android:textStyle="bold" />

	    	</LinearLayout >

		    <LinearLayout
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:gravity="right"
		        android:orientation="horizontal"
		        android:layout_alignParentTop="true"
		        android:layout_weight="1" >
		        
		        <TextView
				   android:id="@+id/main_row_4_detail_time"
			       	android:layout_width="match_parent"
			       	android:layout_height="wrap_content"
			       	android:ellipsize="end"
			       	android:text="11:11:11 "
			       	android:layout_marginTop="5px"
			       	android:layout_marginRight="5px"
			       	android:textColor="#000000"
			    	android:textSize="15dip"
			    	android:gravity="right" />
			</LinearLayout >
	    </LinearLayout >
		<TextView
		      	android:id="@+id/main_row_4_detail_data"
		       	android:layout_width="match_parent"
		       	android:layout_height="wrap_content"
		       	android:ellipsize="end"
		       	android:text="내용 내용 내용 "
		       	android:layout_marginLeft="20px"
		       	android:layout_marginRight="20px"
                android:layout_marginTop="5px"
                android:layout_marginBottom="10px"
		       	android:textColor="#000000"
		    	android:textSize="15dip"
		    	android:gravity="left" />
		</LinearLayout>
	    

		    
	</LinearLayout >
	
</LinearLayout >


