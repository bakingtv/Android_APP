<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/pattern_1" >
    

    <FrameLayout 
        android:id="@+id/main_screen"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical"
	     >
	     
         <FrameLayout 
	        android:id="@+id/main_layout_1"
		    android:layout_width="match_parent"
		    android:layout_height="726px"   
		    android:layout_marginTop="68px" 
		    android:orientation="vertical"
		     android:focusable="true"
		     android:focusableInTouchMode="true"
		    >
		    <ScrollView
				android:id="@+id/scrollview_1"
			    android:layout_width="match_parent"
			    android:layout_height="match_parent"    
			    android:orientation="vertical"
			    >
			    <FrameLayout 
			        android:id="@+id/main_layout_1_1"
				    android:layout_width="match_parent"
				    android:layout_height="match_parent"   
				    android:orientation="vertical"
				    >
				     <ImageView
		                android:id="@+id/backimage"
		                android:layout_width="match_parent"
		                android:layout_marginTop="10px"
		                android:layout_height="477px"
		                android:layout_gravity="left"
		               android:background="@drawable/join_contents" />
					   
					<EditText
						android:id="@+id/login_id_data_3"
						 android:layout_width="200px"
						 android:layout_height="wrap_content"
						 android:layout_marginLeft="155px"
						 android:layout_marginTop="40px"
						 android:background="@null"
						 android:textColor="#000000"
						 android:hint="아이디"
						 android:textCursorDrawable="@null"
						 android:textSize="20px" />
					 
					  <EditText
						  android:id="@+id/login_pass_data_3"
						  android:layout_width="280px"
						  android:layout_height="wrap_content"
						  android:layout_marginLeft="155px"
						  android:layout_marginTop="95px"
						  android:background="@null"
						  android:hint="비밀번호"
						  android:inputType="textPassword"
						  android:textColor="#000000"
						  android:textCursorDrawable="@null"
						  android:textSize="20px" />
					  
					  
					  <EditText
						  android:id="@+id/login_pass_data_1"
						  android:layout_width="280px"
						  android:layout_height="wrap_content"
						  android:layout_marginLeft="155px"
						  android:layout_marginTop="150px"
						  android:background="@null"
						  android:hint="비밀번호 확인"
						  android:inputType="textPassword"
						  android:textColor="#000000"
						  android:textCursorDrawable="@null"
						  android:textSize="20px" />
					  

					  
					  <EditText
							android:id="@+id/login_nick_data"
							 android:layout_width="260px"
							 android:layout_height="wrap_content"
							 android:layout_marginLeft="155px"
							 android:layout_marginTop="205px"
							 android:background="@null"
							 android:textColor="#000000"
							 android:hint="닉네임"
							 android:textCursorDrawable="@null"
							 android:textSize="20px" />
					  
					  <EditText
						  android:id="@+id/login_email_data"
						  android:layout_width="260px"
						  android:layout_height="wrap_content"
						  android:layout_marginLeft="155px"
						  android:layout_marginTop="260px"
						  android:background="@null"
						  android:hint="이메일"
						  android:inputType="textEmailAddress"
						  android:textColor="#000000"
						  android:textCursorDrawable="@null"
						  android:textSize="20px" />
					  

					 
					  <ImageView
						android:id="@+id/id_check"
						android:layout_width="85px"
						android:layout_height="43px"
						android:layout_gravity="left"
						android:layout_marginLeft="363px"
						android:layout_marginTop="26px"
						android:background="@drawable/id_error_bt_1" />
					  
					  <ImageView
						android:id="@+id/checkbox_1"
						android:layout_width="34px"
						android:layout_height="34px"
						android:layout_gravity="left"
						android:layout_marginLeft="10px"
						android:layout_marginTop="355px"
						android:background="@drawable/check_on" />
					  <ImageView
						android:id="@+id/checkbox_2"
						android:layout_width="34px"
						android:layout_height="34px"
						android:layout_gravity="left"
						android:layout_marginLeft="10px"
						android:layout_marginTop="395px"
						android:background="@drawable/check_off" />
					  <ImageView
						android:id="@+id/checkbox_3"
						android:layout_width="34px"
						android:layout_height="34px"
						android:layout_gravity="left"
						android:layout_marginLeft="10px"
						android:layout_marginTop="435px"
						android:background="@drawable/check_off" />
					  
					  <ImageView
						android:id="@+id/detail_btn_1"
						android:layout_width="114px"
						android:layout_height="33px"
						android:layout_gravity="left"
						android:layout_marginLeft="353px"
						android:layout_marginTop="358px"
						android:background="@drawable/detail_bt_1" />
					  <ImageView
						android:id="@+id/detail_btn_2"
						android:layout_width="114px"
						android:layout_height="33px"
						android:layout_gravity="left"
						android:layout_marginLeft="353px"
						android:layout_marginTop="400px"
						android:background="@drawable/detail_bt_1" />
					  <ImageView
						android:id="@+id/detail_btn_3"
						android:layout_width="114px"
						android:layout_height="33px"
						android:layout_gravity="left"
						android:layout_marginLeft="353px"
						android:layout_marginTop="441px"
						android:background="@drawable/detail_bt_1" />
					  
					  
					  <ImageView
						android:id="@+id/next_btn"
						android:layout_width="450px"
						android:layout_height="65px"
						android:layout_gravity="left"
						android:layout_marginLeft="21px"
						android:layout_marginTop="512px"
						android:background="@drawable/regist_next_bt_1" />
		    	</FrameLayout>
			   
			    
			    
			    
			 </ScrollView>
             

              
                          


	    </FrameLayout>

	   <TextView
			android:id="@+id/title_text"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:textSize="38px" 
			android:textColor="#FF000000"
			android:text="회원가입"
			android:layout_marginTop="10px"
			android:gravity="center_horizontal"/>
    	<ImageView
			android:id="@+id/title_line"
			android:layout_width="480px"
			android:layout_height="5px"
			android:background="@drawable/title_bar"
			android:layout_marginTop="68px"
			android:layout_gravity="left"/>

    </FrameLayout>
</RelativeLayout>