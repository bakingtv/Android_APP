<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/pattern_1"
     >
    <TextView
			android:id="@+id/title_text"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:textSize="38px" 
			android:textColor="#FF000000"
			android:text="로그인"
			android:layout_marginTop="10px"
			android:gravity="center_horizontal"/>
    <ImageView
			android:id="@+id/title_line"
			android:layout_width="480px"
			android:layout_height="5px"
			android:background="@drawable/title_bar"
			android:layout_marginTop="68px"
			android:layout_gravity="left"/>
        
    <FrameLayout
        android:id="@+id/main_screen"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >
	     
         <FrameLayout
             android:id="@+id/main_layout_1"
             android:layout_width="match_parent"
             android:layout_height="732px"
             android:layout_marginTop="68px"
             android:focusable="true"
             android:focusableInTouchMode="true"
             android:orientation="vertical"
             android:visibility="gone" >

           <ImageView
                android:id="@+id/member_tab"
                android:layout_width="228px"
                android:layout_height="50px"
                android:layout_gravity="left"
                android:layout_marginLeft="13px"
                android:layout_marginTop="20px"
                android:background="@drawable/user_login_1" />
           <ImageView
                android:id="@+id/no_member_tab"
                android:layout_width="228px"
                android:layout_height="50px"
                android:layout_gravity="left"
                android:layout_marginLeft="240px"
                android:layout_marginTop="20px"
                android:background="@drawable/nonuser_login_2" />
           <ImageView
                android:id="@+id/member_box"
                android:layout_width="454px"
                android:layout_height="298px"
                android:layout_gravity="left"
                android:layout_marginLeft="13px"
                android:layout_marginTop="69px"
                android:background="@drawable/user_login_contents1" />
           <ImageView
                android:id="@+id/login_btn_3"
                android:layout_width="121px"
                android:layout_height="121px"
                android:layout_gravity="left"
                android:layout_marginLeft="320px"
                android:layout_marginTop="106px"
                android:background="@drawable/login_bt" />
           <ImageView
                android:id="@+id/join_btn_2"
                android:layout_width="125px"
                android:layout_height="32px"
                android:layout_gravity="left"
                android:layout_marginLeft="90px"
                android:layout_marginTop="252px"
                 />
             <EditText
                 android:id="@+id/login_id_data_4"
                 android:layout_width="210px"
                 android:layout_height="wrap_content"
                 android:layout_marginLeft="50px"
                 android:layout_marginTop="121px"
                 android:background="@null"
                 android:textColor="#000000"
                 android:hint="아이디"
                 android:textCursorDrawable="@null"
                 android:textSize="20px" />
              <EditText
                  android:id="@+id/login_pass_data_4"
                  android:layout_width="210px"
                  android:layout_height="wrap_content"
                  android:layout_marginLeft="50px"
                  android:layout_marginTop="182px"
                  android:background="@null"
                  android:hint="비밀번호"
                  android:inputType="textPassword"
                  android:textColor="#000000"
                  android:textCursorDrawable="@null"
                  android:textSize="20px" />
              
              <ImageView
                android:id="@+id/id_clear"
                android:layout_width="30px"
                android:layout_height="31px"
                android:layout_gravity="left"
                android:layout_marginLeft="270px"
                android:layout_marginTop="121px"
                android:background="@drawable/clear_bt"/>
              <ImageView
                android:id="@+id/pass_clear"
                android:layout_width="30px"
                android:layout_height="31px"
                android:layout_gravity="left"
                android:layout_marginLeft="270px"
                android:layout_marginTop="181px"
                android:background="@drawable/clear_bt"/>
              <ImageView
                android:id="@+id/auto_login"
                android:layout_width="24px"
                android:layout_height="24px"
                android:layout_gravity="left"
                android:layout_marginLeft="286px"
                android:layout_marginTop="256px"
                android:background="@drawable/check_on"
                 />
	    </FrameLayout>
	    
         <FrameLayout
             android:id="@+id/main_layout_2"
             android:layout_width="match_parent"
             android:layout_height="732px"
             android:layout_marginTop="68px"
             android:orientation="vertical" >

           <ImageView
                android:id="@+id/member_tab_2"
                android:layout_width="228px"
                android:layout_height="50px"
                android:layout_gravity="left"
                android:layout_marginLeft="13px"
                android:layout_marginTop="20px"
                android:background="@drawable/user_login_2" />
           
           <ImageView
                android:id="@+id/no_member_tab_2"
                android:layout_width="228px"
                android:layout_height="50px"
                android:layout_gravity="left"
                android:layout_marginLeft="240px"
                android:layout_marginTop="20px"
                android:background="@drawable/nonuser_login_1" />
           
           <ImageView
                android:id="@+id/member_box2"
                android:layout_width="454px"
                android:layout_height="388px"
                android:layout_gravity="left"
                android:layout_marginLeft="13px"
                android:layout_marginTop="69px"
                android:background="@drawable/user_login_contents2" />
           
           <ImageView
                android:id="@+id/login_btn_2"
                android:layout_width="121px"
                android:layout_height="121px"
                android:layout_gravity="left"
                android:layout_marginLeft="320px"
                android:layout_marginTop="106px"
                android:background="@drawable/login_bt"  />
           
           
           <ImageView
                android:id="@+id/join_btn"
                android:layout_width="125px"
                android:layout_height="32px"
                android:layout_gravity="left"
                android:layout_marginLeft="297px"
                android:layout_marginTop="252px" />
           
           
             <EditText
                 android:id="@+id/login_id_data_2"
                 android:layout_width="210px"
                 android:layout_height="wrap_content"
                 android:layout_marginLeft="50px"
                 android:layout_marginTop="121px"
                 android:background="@null"
                 android:textColor="#000000"
                 android:hint="아이디"
                 android:textCursorDrawable="@null"
                 android:textSize="20px"  />
             
              <EditText
                  android:id="@+id/login_rese_number"
                  android:layout_width="210px"
                 android:layout_height="wrap_content"
                 android:layout_marginLeft="50px"
                 android:layout_marginTop="181px"
                  android:background="@null"
                  android:hint="예약번호"
                  android:textColor="#000000"
                  android:textCursorDrawable="@null"
                  android:textSize="20px" />
              
              <ImageView
                android:id="@+id/id_clear2"
                android:layout_width="30px"
                android:layout_height="31px"
                android:layout_gravity="left"
                android:layout_marginLeft="270px"
                android:layout_marginTop="121px"
                android:background="@drawable/clear_bt"/>
              <ImageView
                android:id="@+id/pass_clear2"
                android:layout_width="30px"
                android:layout_height="31px"
                android:layout_gravity="left"
                android:layout_marginLeft="270px"
                android:layout_marginTop="181px"
                android:background="@drawable/clear_bt"/>
	    </FrameLayout>
    </FrameLayout>
</RelativeLayout>