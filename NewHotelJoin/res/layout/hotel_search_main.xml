<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/pattern_1"
    android:orientation="vertical" >

    <FrameLayout 
        android:id="@+id/main_screen"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical"
	     >
	   
		<TextView
				android:id="@+id/title_text"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:textSize="38px" 
				android:textColor="#FF000000"
				android:text="호텔검색"
				android:layout_marginTop="10px"
				android:gravity="center_horizontal"/>
	    	<ImageView
				android:id="@+id/title_line"
				android:layout_width="480px"
				android:layout_height="5px"
				android:background="@drawable/title_bar"
				android:layout_marginTop="68px"
				android:layout_gravity="left"/>
	    	
	    	<ImageView
				android:id="@+id/title_menu"
				android:layout_width="53px"
				android:layout_height="49px"
				android:layout_marginLeft="417px"
				android:layout_marginTop="10px"
				android:background="@drawable/menu_1"
				android:layout_gravity="left"/>
    	
	     	<ImageView
                android:id="@+id/main_tab1"
                android:layout_width="228px"
                android:layout_height="50px"
                android:layout_gravity="left"
                android:layout_marginLeft="13px"
                android:layout_marginTop="88px"
                android:background="@drawable/hotel_search_1" />
	     	<ImageView
                android:id="@+id/main_tab2"
                android:layout_width="228px"
                android:layout_height="50px"
                android:layout_gravity="left"
                android:layout_marginLeft="239px"
                android:layout_marginTop="88px"
                android:background="@drawable/near_search_1" />
         <FrameLayout 
	        android:id="@+id/main_layout_1"
		    android:layout_width="match_parent"
		    android:layout_height="800px"   
		    android:layout_marginTop="0px" 
		    android:orientation="vertical"
		     android:focusable="true"
		     android:focusableInTouchMode="true"
		    >
		    
             <ImageView
                android:id="@+id/back"
                android:layout_width="454px"
                android:layout_height="489px"
                android:layout_gravity="left"
                android:layout_marginLeft="13px"
                android:layout_marginTop="138px"
                android:background="@drawable/search_contents" />
		    
             <TextView
                 android:id="@+id/hotel_locate"
                 android:layout_width="320px"
                 android:layout_height="wrap_content"
                 android:layout_marginLeft="43px"
                 android:layout_marginTop="193px"
                 android:background="@null"
                 android:textColor="#000000"
                 android:text="현재위치"
                 android:textCursorDrawable="@null"
                 android:textSize="22px" />
             
             <ImageView
                android:id="@+id/gps_now"
                android:layout_width="59px"
                android:layout_height="63px"
                android:layout_gravity="left"
                android:layout_marginLeft="395px"
                android:layout_marginTop="178px"
                android:background="@drawable/gps_1" />
             
             <TextView
                 android:id="@+id/hotel_day"
                 android:layout_width="150px"
                 android:layout_height="25px"
                 android:layout_marginLeft="277px"
                 android:layout_marginTop="302px"
                 android:background="@null"
                 android:textColor="#000000"
                 android:text="2013/01/13"
                 android:textCursorDrawable="@null"
                 android:textSize="23px" />
             
             <TextView
                 android:id="@+id/hotel_day_number"
                 android:layout_width="40px"
                 android:layout_height="wrap_content"
                 android:layout_marginLeft="364px"
                 android:layout_marginTop="371px"
                 android:background="@null"
                 android:gravity="center"
                 android:text="1"
                 android:textColor="#000000"
                 android:textCursorDrawable="@null"
                 android:textSize="23px" />
             
             <TextView
                 android:id="@+id/hotel_room"
                 android:layout_width="40px"
                 android:layout_height="wrap_content"
                 android:layout_marginLeft="364px"
                 android:layout_marginTop="511px"
                 android:background="@null"
                 android:gravity="center"
                 android:text="1"
                 android:textColor="#000000"
                 android:textCursorDrawable="@null"
                 android:textSize="23px" />
             
             
             <TextView
                 android:id="@+id/hotel_room_man"
                 android:layout_width="40px"
                 android:layout_height="wrap_content"
                 android:layout_marginLeft="364px"
                 android:layout_marginTop="580px"
                 android:background="@null"
                 android:gravity="center"
                 android:text="1"
                 android:textColor="#000000"
                 android:textCursorDrawable="@null"
                 android:textSize="23px" />
             
             
		    <ImageView
                android:id="@+id/inn_day_p"
                android:layout_width="24px"
                android:layout_height="23px"
                android:layout_gravity="left"
                android:layout_marginLeft="431px"
                android:layout_marginTop="371px"
                android:background="@drawable/plus_1" />
		    
		     <ImageView
                android:id="@+id/inn_day_m"
                android:layout_width="24px"
                android:layout_height="23px"
                android:layout_gravity="left"
                android:layout_marginLeft="313px"
                android:layout_marginTop="371px"
                android:background="@drawable/minus_1" />
		     
		      <TextView
                 android:id="@+id/check_out"
                 android:layout_width="173px"
                 android:layout_height="wrap_content"
                 android:layout_marginLeft="281px"
               	 android:layout_marginTop="441px"
                 android:background="@null"
                 android:gravity="left"
                 android:text=" 2013/01/16"
                 android:textColor="#000000"
                 android:textCursorDrawable="@null"
                 android:textSize="24px" />
		     
		     
		     <ImageView
                android:id="@+id/inn_count_p"
                android:layout_width="24px"
                android:layout_height="23px"
                android:layout_gravity="left"
                android:layout_marginLeft="431px"
                android:layout_marginTop="511px"
                android:background="@drawable/plus_1" />
		    
		     <ImageView
                android:id="@+id/inn_count_m"
                android:layout_width="24px"
                android:layout_height="23px"
                android:layout_gravity="left"
                android:layout_marginLeft="313px"
                android:layout_marginTop="511px"
                android:background="@drawable/minus_1" />
		     
		     <ImageView
                android:id="@+id/inn_man_p"
                android:layout_width="24px"
                android:layout_height="23px"
                android:layout_gravity="left"
                android:layout_marginLeft="431px"
                android:layout_marginTop="580px"
                android:background="@drawable/plus_1" />
		    
		     <ImageView
                android:id="@+id/inn_man_m"
                android:layout_width="24px"
                android:layout_height="23px"
                android:layout_gravity="left"
                android:layout_marginLeft="313px"
                android:layout_marginTop="580px"
                android:background="@drawable/minus_1" />
		     
             
             <ImageView
                android:id="@+id/search_btn"
                android:layout_width="450px"
                android:layout_height="65px"
                android:layout_gravity="left"
                android:layout_marginLeft="21px"
                android:layout_marginTop="647px"
                android:background="@drawable/hotelsearch_g_bt_1" />
	    </FrameLayout>

          <FrameLayout
              android:id="@+id/main_layout_3"
              android:layout_width="454px"
              android:layout_height="match_parent"
              android:layout_marginLeft="13px"
              android:layout_marginTop="138px"
              android:orientation="vertical"
              android:visibility="gone" >

             <fragment xmlns:android="http://schemas.android.com/apk/res/android"
		           android:id="@+id/map_view"
		           android:layout_width="match_parent"
		           android:layout_height="match_parent"
		           class="com.google.android.gms.maps.SupportMapFragment"/>

         </FrameLayout>    
         <FrameLayout
             android:id="@+id/main_layout_2"
             android:layout_width="match_parent"
             android:layout_height="446px"
             android:layout_marginTop="0px"
             android:background="@drawable/pattern_1"
             android:focusable="true"
             android:focusableInTouchMode="true"
             android:orientation="vertical"
             android:visibility="gone" >
            
            <ImageView
				android:id="@+id/title_line_2"
				android:layout_width="480px"
				android:layout_height="5px"
				android:background="@drawable/title_bar"
				android:layout_marginTop="68px"
				android:layout_gravity="left"/>
           <ImageView
                android:id="@+id/search_tab_1_1"
                android:layout_width="152px"
                android:layout_height="55px"
                android:layout_gravity="left"
                android:layout_marginLeft="13px"
                android:layout_marginTop="7px"
                android:background="@drawable/korea_1" />
           
           <ImageView
                android:id="@+id/search_tab_2_1"
                android:layout_width="152px"
                android:layout_height="55px"
                android:layout_gravity="left"
                android:layout_marginLeft="165px"
                android:layout_marginTop="7px"
                android:background="@drawable/abroad_1" />
           <ImageView
                android:id="@+id/search_tab_3_1"
                android:layout_width="152px"
                android:layout_height="55px"
                android:layout_gravity="left"
                android:layout_marginLeft="317px"
                android:layout_marginTop="7px"
                android:background="@drawable/recently_1" />
           <ImageView
                android:id="@+id/search_text_bar_1"
                android:layout_width="480px"
                android:layout_height="78px"
                android:layout_gravity="left"
                android:layout_marginLeft="0px"
                android:layout_marginTop="73px"
                android:background="@drawable/search_bar" />
            <ImageView
                android:id="@+id/search_btn_1"
                android:layout_width="59px"
                android:layout_height="63px"
                android:layout_gravity="left"
                android:layout_marginLeft="9px"
                android:layout_marginTop="81px"
                android:background="@drawable/search_bt_1" />
            <EditText
				android:id="@+id/search_text_1"
				android:layout_width="300px"
				android:layout_height="wrap_content"
				android:layout_marginLeft="91px"
				android:layout_marginTop="95px"
				android:background="@null"
				android:textColor="#000000"
				android:hint="검색어를 입력하세요."
				android:textCursorDrawable="@null"
				android:textSize="20px" />
            <ListView
			     android:id="@+id/search_list"
			     android:layout_width="match_parent"
			     android:layout_height="260px"
			     android:background="#FFFFFF"
			     android:layout_marginTop="151px"
			      />
            <ImageView
                android:id="@+id/page_up_1"
                android:layout_width="match_parent"
                android:layout_height="31px"
                android:layout_gravity="left"
                android:layout_marginLeft="0px"
                android:layout_marginTop="415px"
                android:background="@drawable/upper_line" />
         </FrameLayout>
    </FrameLayout>
</RelativeLayout>