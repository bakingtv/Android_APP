<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    

    <FrameLayout 
        android:id="@+id/main_screen"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical"
	     >
	     
         <FrameLayout 
	        android:id="@+id/main_layout"
		    android:layout_width="match_parent"
		    android:layout_height="800px"   
		    android:orientation="vertical"
		     android:focusable="true"
		     android:focusableInTouchMode="true"
		     android:background="@drawable/reser_bg"
		    >
		    <ImageView
                android:id="@+id/call1_btn"
                android:layout_width="218px"
                android:layout_height="68px"
                android:layout_gravity="left"
                android:layout_marginLeft="14px"
                android:layout_marginTop="517px"
                android:background="@drawable/reserve_korea_1_bt" />
           
           
           <ImageView
                android:id="@+id/call2_btn"
                android:layout_width="218px"
                android:layout_height="68px"
                android:layout_gravity="left"
                android:layout_marginLeft="248px"
                android:layout_marginTop="517px"
                android:background="@drawable/reserve_global_1_bt" />
           
           
           <ListView
			     android:id="@+id/main_list"
			     android:layout_width="match_parent"
			     android:layout_height="257px"
			     android:background="#FFFFFF"
			     android:layout_marginTop="68px"
			      />
           
		    <FrameLayout
				android:id="@+id/title_bar"
			    android:layout_width="match_parent"
			    android:layout_height="68px"
			    android:background="@drawable/top_tap_bg"
			    android:orientation="vertical"
			    >
					    
			    <TextView
			    	android:id="@+id/title_logo"
			        android:layout_width="200px"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="140px"
			        android:layout_marginTop="12px"
			        android:background="@null"
			        android:gravity="center"
			        android:text="예약확인"
			        android:textColor="#000000"
			        android:textCursorDrawable="@null"
			        android:textSize="40px" />
			            
			   <ImageView
			        android:id="@+id/sub_bar"
			        android:layout_width="48px"
			        android:layout_height="48px"
			        android:layout_gravity="left"
			        android:layout_marginLeft="418px"
			        android:layout_marginTop="12px"
			        android:background="@drawable/profile_bt_1" 
			        android:visibility="gone" />
			              
			</FrameLayout>
	
	    </FrameLayout>
    </FrameLayout>

    <LinearLayout
        android:id="@+id/bottom_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_gravity="bottom"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical" >

		<LinearLayout
			android:id="@+id/bottom_menu_1"
			android:layout_width="480px"
			android:layout_height="74px"
			android:orientation="horizontal"   >
			<ImageView
					android:id="@+id/bottom_1"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/bt_1_1_bt"
					android:layout_gravity="left"/>
			<ImageView
					android:id="@+id/bottom_2"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/bt_2_1_bt"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_3"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/bt_3_1_bt"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_4"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/bt_4_1_bt"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_5"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/bt_5_1_bt"
					android:layout_gravity="left"/>
		</LinearLayout>
		

	 </LinearLayout>
</RelativeLayout>