<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    

    <FrameLayout 
        android:id="@+id/main_screen"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical"
	     >
	     
         <FrameLayout 
	        android:id="@+id/main_layout"
		    android:layout_width="match_parent"
		    android:layout_height="724px"   
		    android:orientation="vertical"
		     android:background="#FFFFFF"
		     android:focusable="true"
		     android:focusableInTouchMode="true"
		    >
		    <com.euiweonjeong.base.view.ObservableScrollView
				android:id="@+id/scrollview_1"
			    android:layout_width="match_parent"
			    android:layout_height="match_parent"    
			    android:orientation="vertical"
			    android:background="#FFFFFF"
			    >
			    <FrameLayout 
			        android:id="@+id/main_layout_1_1"
				    android:layout_width="match_parent"
				    android:layout_height="match_parent"   
				    android:orientation="vertical"
				     android:background="#FFFFFF"
				    >
				     <ImageView
		                android:id="@+id/backimage"
		                android:layout_width="480px"
		                android:layout_height="1344px"
		                android:layout_gravity="left"
		               android:background="@drawable/k_bg" />
				     <ImageView
		                android:id="@+id/backimage2"
		                android:layout_width="480px"
		                android:layout_height="941px"
		                android:layout_marginTop="401px"
		                android:layout_gravity="left"
		               android:background="@drawable/hotelreserve_reserve_k_bg3" />
				     <FrameLayout
			             android:id="@+id/title_bar"
			             android:layout_width="match_parent"
			             android:layout_height="68px"
			             android:background="@drawable/top_tap_bg"
			             android:orientation="vertical"
			             >
					    
			            <TextView
			                 android:id="@+id/title_logo"
			                 android:layout_width="200px"
			                 android:layout_height="wrap_content"
			                 android:layout_marginLeft="140px"
			               	 android:layout_marginTop="12px"
			                 android:background="@null"
			                 android:gravity="center"
			                 android:text="상세정보"
			                 android:textColor="#000000"
			                 android:textCursorDrawable="@null"
			                 android:textSize="40px" />
			            
			            <ImageView
			                android:id="@+id/sub_bar"
			                android:layout_width="48px"
			                android:layout_height="48px"
			                android:layout_gravity="left"
			                android:layout_marginLeft="418px"
			                android:layout_marginTop="12px"
			                android:background="@drawable/profile_bt_1" 
			                android:visibility="gone" />
			              
				    </FrameLayout>
				    
				      <ImageView
						android:id="@+id/login_btn"
						android:layout_width="450px"
						android:layout_height="65px"
						android:layout_gravity="left"
						android:layout_marginLeft="16px"
						android:layout_marginTop="410px"
						android:background="@drawable/login_btn" />
				      
				      <TextView
			                 android:id="@+id/roomname"
			                 android:layout_width="320px"
			                 android:layout_height="wrap_content"
			                 android:layout_marginLeft="140px"
			               	 android:layout_marginTop="78px"
			                 android:background="@null"
			                 android:text="방 번호!!"
			                 android:textColor="#000000"
			                 android:textCursorDrawable="@null"
			                 android:textSize="20px" android:singleLine = "true" />
				      <TextView
			                 android:id="@+id/roomday"
			                 android:layout_width="320px"
			                 android:layout_height="wrap_content"
			                 android:layout_marginLeft="140px"
			               	 android:layout_marginTop="108px"
			                 android:background="@null"
			                 android:text="방 번호!!"
			                 android:textColor="#000000"
			                 android:textCursorDrawable="@null"
			                 android:textSize="20px"  android:singleLine = "true"/>
				      
				      <TextView
			                 android:id="@+id/roomcount"
			                 android:layout_width="320px"
			                 android:layout_height="wrap_content"
			                 android:layout_marginLeft="140px"
			               	 android:layout_marginTop="138px"
			                 android:background="@null"
			                 android:text="방 번호!!"
			                 android:textColor="#000000"
			                 android:textCursorDrawable="@null"
			                 android:textSize="20px"  android:singleLine = "true"/>
				      
				      <TextView
			                 android:id="@+id/roomman"
			                 android:layout_width="320px"
			                 android:layout_height="wrap_content"
			                 android:layout_marginLeft="140px"
