<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    

    <FrameLayout 
        android:id="@+id/main_screen"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical"
	     >
	     
         <FrameLayout 
	        android:id="@+id/main_layout"
		    android:layout_width="match_parent"
		    android:layout_height="800px"   
		     android:layout_marginTop="68px" 
		    android:orientation="vertical"
		     android:background="@drawable/common_bg"
		     android:focusable="true"
		     android:focusableInTouchMode="true"
		    >
		     <ImageView
                android:id="@+id/image_bar_1_1"
                android:layout_width="match_parent"
                android:layout_height="65px"
                android:layout_gravity="left"
                android:background="@drawable/tap_bt_bg" />

           <ImageView
                android:id="@+id/tab_1"
                android:layout_width="193px"
                android:layout_height="47px"
                android:layout_gravity="left"
                android:layout_marginLeft="11px"
                android:layout_marginTop="8px"
                android:background="@drawable/a_1_bt" />
           <ImageView
                android:id="@+id/tab_2"
                android:layout_width="193px"
                android:layout_height="47px"
                android:layout_gravity="left"
                android:layout_marginLeft="201px"
                android:layout_marginTop="8px"
                android:background="@drawable/b_1_bt" />
           <ImageView
                android:id="@+id/tab_3"
                android:layout_width="78px"
                android:layout_height="47px"
                android:layout_gravity="left"
                android:layout_marginLeft="393px"
                android:layout_marginTop="8px"
                android:background="@drawable/c_1_bt" />

           <ListView
			     android:id="@+id/search_list"
			     android:layout_width="match_parent"
			     android:layout_height="590px"
			     android:background="#FFFFFF"
			     android:layout_marginTop="68px"
			      />
           

	    </FrameLayout>
	    
 
	    
	    
         <FrameLayout
             android:id="@+id/title_bar"
             android:layout_width="match_parent"
             android:layout_height="68px"
             android:background="@drawable/top_tap_bg"
             android:orientation="vertical"
             >
		    
            <TextView
                 android:id="@+id/title_logo"
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content"
                 
               	 android:layout_marginTop="12px"
                 android:background="@null"
                 android:gravity="center"
                 android:text="00건의 검색 결과"
                 android:textColor="#000000"
                 android:textCursorDrawable="@null"
                 android:textSize="40px" />
            
            <ImageView
                android:id="@+id/sub_bar"
                android:layout_width="48px"
                android:layout_height="48px"
                android:layout_gravity="left"
                android:layout_marginLeft="418px"
                android:layout_marginTop="12px"
                android:background="@drawable/setting"
                android:visibility="gone" />
              
	    </FrameLayout>
	    
 
    </FrameLayout>

    <FrameLayout
        android:id="@+id/popup_menu"
        android:layout_width="match_parent"
        android:layout_height="800px"
        android:background="@drawable/filter_bg"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:orientation="vertical"
        >

		<TextView
       		android:id="@+id/popup_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12px"
            android:background="@null"
            android:gravity="center"
            android:text="필터링"
            android:textColor="#000000"
            android:textCursorDrawable="@null"
            android:textSize="40px" />
		<TextView
       		android:id="@+id/popup_result"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="70px"
            android:background="@null"
            android:gravity="center"
            android:text=""
            android:textColor="#FFFFFF"
            android:textCursorDrawable="@null"
            android:textSize="20px" />
		
		
		
		<ImageView
            android:id="@+id/search_btn_1"
            android:layout_width="67px"
            android:layout_height="45px"
            android:layout_gravity="left"
            android:layout_marginLeft="23px"
            android:layout_marginTop="109px"
            android:background="@drawable/search_btn" />
        <EditText
			android:id="@+id/search_text_1"
			android:layout_width="300px"
			android:layout_height="wrap_content"
			android:layout_marginLeft="100px"
			android:layout_marginTop="120px"
			android:background="@null"
			android:textColor="#000000"
			android:hint="검색어를 입력하세요."
			android:textCursorDrawable="@null"
			android:textSize="20px" />
        
        <ImageView
            android:id="@+id/popup_btn1"
            android:layout_width="480px"
            android:layout_height="77px"
            android:layout_gravity="left"
            android:layout_marginLeft="0px"
            android:layout_marginTop="159px"
            android:background="@drawable/hotel_lv1"
            />
        <ImageView
            android:id="@+id/popup_btn2"
            android:layout_width="480px"
            android:layout_height="77px"
            android:layout_gravity="left"
            android:layout_marginLeft="0px"
            android:layout_marginTop="236px"
            android:background="@drawable/howtowork1"
            />
        
        <TextView
       		android:id="@+id/popup_btn_text1"
            android:layout_width="100px"
            android:layout_height="wrap_content"
            android:layout_marginTop="180px"
            android:layout_marginLeft="300px"
            android:background="@null"
            android:gravity="center"
            android:text="전체선택"
            android:textColor="#000000"
            android:textCursorDrawable="@null"
            android:textSize="25px" />
        
        <TextView
       		android:id="@+id/popup_btn_text2"
            android:layout_width="100px"
            android:layout_height="wrap_content"
            android:layout_marginTop="260px"
            android:layout_marginLeft="300px"
            android:background="@null"
            android:gravity="center"
            android:text="전체선택"
            android:textColor="#000000"
            android:textCursorDrawable="@null"
            android:textSize="25px" />
        <ImageView
			android:id="@+id/checkbox_1"
			android:layout_width="40px"
			android:layout_height="40px"
			android:layout_gravity="left"
			android:layout_marginLeft="180px"
			android:layout_marginTop="334px"
			android:background="@drawable/checkbox_1" />
        <ImageView
			android:id="@+id/checkbox_2"
			android:layout_width="40px"
			android:layout_height="40px"
			android:layout_gravity="left"
			android:layout_marginLeft="420px"
			android:layout_marginTop="334px"
			android:background="@drawable/checkbox_1" />
        
        <ImageView
            android:id="@+id/popup_price_back1"
            android:layout_width="115px"
            android:layout_height="44px"
            android:layout_gravity="left"
            android:layout_marginLeft="40px"
            android:layout_marginTop="480px"
            android:background="@drawable/progress_m_1"
            />
        
        <ImageView
            android:id="@+id/popup_price_back2"
            android:layout_width="115px"
            android:layout_height="44px"
            android:layout_gravity="left"
            android:layout_marginLeft="320px"
            android:layout_marginTop="480px"
            android:background="@drawable/progress_m_1"
            />
        
        <TextView
       		android:id="@+id/popup_price_1"
            android:layout_width="115px"
            android:layout_height="44px"
            android:layout_marginLeft="40px"
            android:layout_marginTop="480px"
            android:background="@null"
            android:gravity="center"
            android:text="20,000"
            android:textColor="#000000"
            android:textCursorDrawable="@null"
            android:textSize="18px" />
        
        <TextView
       		android:id="@+id/popup_price_2"
            android:layout_width="115px"
            android:layout_height="44px"
            android:layout_marginLeft="320px"
            android:layout_marginTop="480px"
            android:background="@null"
            android:gravity="center"
            android:text="20,000,000"
            android:textColor="#000000"
            android:textCursorDrawable="@null"
            android:textSize="18px" />
       <ImageView
            android:id="@+id/search_btn"
            android:layout_width="219px"
            android:layout_height="74px"
            android:layout_gravity="left"
            android:layout_marginLeft="241px"
            android:layout_marginTop="613px"
            android:background="@drawable/s_1_bt" />
       
       <ImageView
            android:id="@+id/init_btn"
            android:layout_width="219px"
            android:layout_height="74px"
            android:layout_gravity="left"
            android:layout_marginLeft="17px"
            android:layout_marginTop="613px"
            android:background="@drawable/f_1_bt" />
       
       
       <FrameLayout
             android:id="@+id/main_layout_2"
             android:layout_width="match_parent"
             android:layout_height="545px"
             android:layout_marginTop="0px"
             android:focusable="true"
             android:focusableInTouchMode="true"
             android:orientation="vertical"
              >
            <ListView
			     android:id="@+id/search_list_2"
			     android:layout_width="match_parent"
			     android:layout_height="348px"
			     android:background="#FFFFFF"
			     android:layout_marginTop="160px"
			      />
            
            <ImageView
                android:id="@+id/page_up_1"
                android:layout_width="match_parent"
                android:layout_height="50px"
                android:layout_gravity="left"
                android:layout_marginLeft="0px"
                android:layout_marginTop="502px"
                android:background="@drawable/page_up_1" />
            
           
         </FrameLayout>
        

	 </FrameLayout>
	 

    <LinearLayout
        android:id="@+id/bottom_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_gravity="bottom"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical" >

		<LinearLayout
			android:id="@+id/bottom_menu_1"
			android:layout_width="480px"
			android:layout_height="74px"
			android:orientation="horizontal"   >
			<ImageView
					android:id="@+id/bottom_1"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/bt_1_1_bt"
					android:layout_gravity="left"/>
			<ImageView
					android:id="@+id/bottom_2"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/bt_2_1_bt"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_3"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/bt_3_1_bt"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_4"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/bt_4_1_bt"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_5"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/bt_5_1_bt"
					android:layout_gravity="left"/>
		</LinearLayout>
		

	 </LinearLayout>

</RelativeLayout>