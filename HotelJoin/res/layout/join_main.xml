<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    

    <FrameLayout 
        android:id="@+id/main_screen"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical"
	     >
	     
         <FrameLayout 
	        android:id="@+id/main_layout_1"
		    android:layout_width="match_parent"
		    android:layout_height="726px"   
		    android:layout_marginTop="68px" 
		    android:orientation="vertical"
		     android:background="#FFFFFF"
		     android:focusable="true"
		     android:focusableInTouchMode="true"
		    >
		    <ScrollView
				android:id="@+id/scrollview_1"
			    android:layout_width="match_parent"
			    android:layout_height="match_parent"    
			    android:orientation="vertical"
			    android:background="#FFFFFF"
			    >
			    <FrameLayout 
			        android:id="@+id/main_layout_1_1"
				    android:layout_width="match_parent"
				    android:layout_height="match_parent"   
				    android:orientation="vertical"
				     android:background="#FFFFFF"
				    >
				     <ImageView
		                android:id="@+id/backimage"
		                android:layout_width="match_parent"
		                android:layout_height="798px"
		                android:layout_gravity="left"
		               android:background="@drawable/regist_bg" />
					   
					<EditText
						android:id="@+id/login_id_data_3"
						 android:layout_width="200px"
						 android:layout_height="wrap_content"
						 android:layout_marginLeft="155px"
						 android:layout_marginTop="30px"
						 android:background="@null"
						 android:textColor="#000000"
						 android:hint="아이디"
						 android:textCursorDrawable="@null"
						 android:textSize="20px" />
					 
					  <EditText
						  android:id="@+id/login_pass_data_3"
						  android:layout_width="300px"
						  android:layout_height="wrap_content"
						  android:layout_marginLeft="155px"
						  android:layout_marginTop="105px"
						  android:background="@null"
						  android:hint="비밀번호"
						  android:inputType="textPassword"
						  android:textColor="#000000"
						  android:textCursorDrawable="@null"
						  android:textSize="20px" />
					  
					  
					  <EditText
						  android:id="@+id/login_pass_data_1"
						  android:layout_width="300px"
						  android:layout_height="wrap_content"
						  android:layout_marginLeft="155px"
						  android:layout_marginTop="180px"
						  android:background="@null"
						  android:hint="비밀번호 확인"
						  android:inputType="textPassword"
						  android:textColor="#000000"
						  android:textCursorDrawable="@null"
						  android:textSize="20px" />
					  
					  <EditText
							android:id="@+id/login_name_data"
							 android:layout_width="200px"
							 android:layout_height="wrap_content"
							 android:layout_marginLeft="155px"
							 android:layout_marginTop="255px"
							 android:background="@null"
							 android:textColor="#000000"
							 android:hint="이름"
							 android:textCursorDrawable="@null"
							 android:textSize="20px" />
					  
					  <EditText
							android:id="@+id/login_nick_data"
							 android:layout_width="200px"
							 android:layout_height="wrap_content"
							 android:layout_marginLeft="155px"
							 android:layout_marginTop="335px"
							 android:background="@null"
							 android:textColor="#000000"
							 android:hint="닉네임"
							 android:textCursorDrawable="@null"
							 android:textSize="20px" />
					  
					  <EditText
						  android:id="@+id/login_email_data"
						  android:layout_width="300px"
						  android:layout_height="wrap_content"
						  android:layout_marginLeft="155px"
						  android:layout_marginTop="410px"
						  android:background="@null"
						  android:hint="이메일"
						  android:inputType="textEmailAddress"
						  android:textColor="#000000"
						  android:textCursorDrawable="@null"
						  android:textSize="20px" />
					  
					  <EditText
						  android:id="@+id/login_phone_number_data"
						  android:layout_width="300px"
						  android:layout_height="wrap_content"
						  android:layout_marginLeft="155px"
						  android:layout_marginTop="490px"
						  android:background="@null"
						  android:hint="휴대폰 번호"
						  android:inputType="phone"
						  android:textColor="#000000"
						  android:textCursorDrawable="@null"
						  android:textSize="20px" />
					 
					  <ImageView
						android:id="@+id/id_check"
						android:layout_width="85px"
						android:layout_height="43px"
						android:layout_gravity="left"
						android:layout_marginLeft="385px"
						android:layout_marginTop="16px"
						android:background="@drawable/id_error_bt_1" />
					  
					  <ImageView
						android:id="@+id/checkbox_1"
						android:layout_width="40px"
						android:layout_height="40px"
						android:layout_gravity="left"
						android:layout_marginLeft="21px"
						android:layout_marginTop="544px"
						android:background="@drawable/checkbox_1" />
					  <ImageView
						android:id="@+id/checkbox_2"
						android:layout_width="40px"
						android:layout_height="40px"
						android:layout_gravity="left"
						android:layout_marginLeft="21px"
						android:layout_marginTop="596px"
						android:background="@drawable/checkbox_1" />
					  <ImageView
						android:id="@+id/checkbox_3"
						android:layout_width="40px"
						android:layout_height="40px"
						android:layout_gravity="left"
						android:layout_marginLeft="21px"
						android:layout_marginTop="647px"
						android:background="@drawable/checkbox_1" />
					  
					  <ImageView
						android:id="@+id/detail_btn_1"
						android:layout_width="114px"
						android:layout_height="33px"
						android:layout_gravity="left"
						android:layout_marginLeft="353px"
						android:layout_marginTop="548px"
						android:background="@drawable/detail_bt_1" />
					  <ImageView
						android:id="@+id/detail_btn_2"
						android:layout_width="114px"
						android:layout_height="33px"
						android:layout_gravity="left"
						android:layout_marginLeft="353px"
						android:layout_marginTop="600px"
						android:background="@drawable/detail_bt_1" />
					  <ImageView
						android:id="@+id/detail_btn_3"
						android:layout_width="114px"
						android:layout_height="33px"
						android:layout_gravity="left"
						android:layout_marginLeft="353px"
						android:layout_marginTop="651px"
						android:background="@drawable/detail_bt_1" />
					  
					  
					  <ImageView
						android:id="@+id/next_btn"
						android:layout_width="450px"
						android:layout_height="65px"
						android:layout_gravity="left"
						android:layout_marginLeft="21px"
						android:layout_marginTop="712px"
						android:background="@drawable/regist_next_bt_1" />
		    	</FrameLayout>
			   
			    
			    
			    
			 </ScrollView>
             

              
                          


	    </FrameLayout>

	    
	    
         <FrameLayout
             android:id="@+id/title_bar"
             android:layout_width="match_parent"
             android:layout_height="68px"
             android:background="@drawable/top_tap_bg"
             android:orientation="vertical"
             >
		    
            <TextView
                 android:id="@+id/title_logo"
                 android:layout_width="200px"
                 android:layout_height="wrap_content"
                 android:layout_marginLeft="140px"
               	 android:layout_marginTop="12px"
                 android:background="@null"
                 android:gravity="center"
                 android:text="회원가입"
                 android:textColor="#000000"
                 android:textCursorDrawable="@null"
                 android:textSize="40px" />
            
            <ImageView
                android:id="@+id/sub_bar"
                android:layout_width="48px"
                android:layout_height="48px"
                android:layout_gravity="left"
                android:layout_marginLeft="418px"
                android:layout_marginTop="12px"
                android:background="@drawable/profile_bt_1" 
                android:visibility="gone" />
              
	    </FrameLayout>

    </FrameLayout>

    <LinearLayout
        android:id="@+id/bottom_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_gravity="bottom"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical" >

		<LinearLayout
			android:id="@+id/bottom_menu_1"
			android:layout_width="480px"
			android:layout_height="74px"
			android:orientation="horizontal"   >
			<ImageView
					android:id="@+id/bottom_1"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/bt_1_1_bt"
					android:layout_gravity="left"/>
			<ImageView
					android:id="@+id/bottom_2"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/bt_2_1_bt"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_3"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/bt_3_1_bt"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_4"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/bt_4_1_bt"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_5"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/bt_5_1_bt"
					android:layout_gravity="left"/>
		</LinearLayout>
		

	 </LinearLayout>
</RelativeLayout>