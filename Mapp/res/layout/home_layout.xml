<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    

    <FrameLayout 
        android:id="@+id/home_screen"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical"
	     >
         <FrameLayout 
	        android:id="@+id/title_bar"
		    android:layout_width="match_parent"
		    android:layout_height="66px"
		    android:orientation="vertical"
		    android:background="@drawable/title_bar_bg" >
		    
            <ImageView
                android:id="@+id/title_icon"
                android:layout_width="45px"
                android:layout_height="45px"
                android:layout_gravity="left"
                android:layout_marginLeft="31px"
                android:layout_marginTop="10px"
                android:background="@drawable/home_icon" />
            
            <TextView
		      	android:id="@+id/title_name"
		       	android:layout_width="match_parent"
		       	android:layout_height="wrap_content"
		       	android:layout_marginLeft="98px"
                android:layout_marginTop="10px"
		       	android:ellipsize="end"
		       	android:lines="1"
		       	android:maxLines="1"
		       	android:text="Mapp"
		       	android:textColor="#FFFFFF"
		    	android:textSize="15dip" />
            
            <TextView
		      	android:id="@+id/title_desc"
		       	android:layout_width="match_parent"
		       	android:layout_height="wrap_content"
		       	android:layout_marginLeft="99px"
                android:layout_marginTop="34px"
		       	android:ellipsize="end"
		       	android:lines="1"
		       	android:maxLines="1"
		       	android:text=""
		       	android:textColor="#FFFF00"
		    	android:textSize="15dip" />

	    </FrameLayout>
	    
         
         <FrameLayout 
	        android:id="@+id/home_layout"
		    android:layout_width="match_parent"
		    android:layout_height="match_parent"    
		    android:layout_marginTop="68px"
		    android:orientation="vertical"
		    android:background="@drawable/bg" >
            
             <ImageView
                 android:id="@+id/home_logo"
                 android:layout_width="327px"
                 android:layout_height="121px"
                 android:layout_gravity="center|top"
                 android:layout_marginTop="50px"
                 android:background="@drawable/main_logo"
                />
             
		    
             <com.euiweonjeong.base.view.GalleryForOneFling
                 android:id="@+id/home_stage_img"
                 android:layout_width="465px"
                 android:layout_height="261px"
                 android:layout_gravity="left"
                 android:layout_marginLeft="7px"
                 android:layout_marginTop="207px"
                 android:background="@drawable/home_audition_vacuum" />
             
             
             <TextView
		      	android:id="@+id/home_stage_text"
		       	android:layout_width="match_parent"
		       	android:layout_height="wrap_content"
		       	android:layout_marginLeft="16px"
                android:layout_marginTop="442px"
		       	android:ellipsize="end"
		       	android:lines="1"
		       	android:maxLines="1"
		       	android:text="가로수 그늘 아래"
		       	android:textColor="#FFFFFF"
		    	android:textSize="15dip" />
             
             <ImageView
                 android:id="@+id/home_stage_icon"
                 android:layout_width="59px"
                 android:layout_height="22px"
                 android:layout_gravity="left"
                 android:layout_marginLeft="7px"
                 android:layout_marginTop="20px"
                 android:background="@drawable/stage_tag"
                 android:visibility="gone" />
             
             <ImageView
                android:id="@+id/home_stage_btn_1"
                android:layout_width="9px"
                android:layout_height="9px"
                android:layout_gravity="left"
                android:layout_marginLeft="420px"
                android:layout_marginTop="450px"
                android:background="@drawable/home_control" />
             
             <ImageView
                android:id="@+id/home_stage_btn_2"
                android:layout_width="9px"
                android:layout_height="9px"
                android:layout_gravity="left"
                android:layout_marginLeft="437px"
                android:layout_marginTop="450px"
                android:background="@drawable/home_control" />
             
             
             <ImageView
                android:id="@+id/home_stage_btn_3"
                android:layout_width="9px"
                android:layout_height="9px"
                android:layout_gravity="left"
                android:layout_marginLeft="454px"
                android:layout_marginTop="450px"
                android:background="@drawable/home_control" />
             
             <ImageView
                android:id="@+id/home_stage_btn_1_1"
                android:layout_width="9px"
                android:layout_height="9px"
                android:layout_gravity="left"
                android:layout_marginLeft="420px"
                android:layout_marginTop="450px"
                android:background="@drawable/home_control_selected" />
             
             <ImageView
                android:id="@+id/home_stage_btn_2_1"
                android:layout_width="9px"
                android:layout_height="9px"
                android:layout_gravity="left"
                android:layout_marginLeft="437px"
                android:layout_marginTop="450px"
                android:background="@drawable/home_control_selected" />
             
             
             <ImageView
                android:id="@+id/home_stage_btn_3_1"
                android:layout_width="9px"
                android:layout_height="9px"
                android:layout_gravity="left"
                android:layout_marginLeft="454px"
                android:layout_marginTop="450px"
                android:background="@drawable/home_control_selected" />
             
             <ImageView
                 android:id="@+id/home_album_img"
                 android:layout_width="230px"
                 android:layout_height="230px"
                 android:layout_gravity="left"
                 android:layout_marginLeft="7px"
                 android:layout_marginTop="273px"
                 android:background="@drawable/home_album_vacuum"
                 android:visibility="gone" />
             
             <ImageView
                 android:id="@+id/home_album_icon"
                 android:layout_width="59px"
                 android:layout_height="22px"
                 android:layout_gravity="left"
                 android:layout_marginLeft="7px"
                 android:layout_marginTop="287px"
                 android:background="@drawable/album_tag"
                 android:visibility="gone" />
             
             <ImageView
                 android:id="@+id/home_square_img"
                 android:layout_width="230px"
                 android:layout_height="230px"
                 android:layout_gravity="left"
                 android:layout_marginLeft="242px"
                 android:layout_marginTop="273px"
                 android:background="@drawable/home_album_vacuum"
                 android:visibility="gone" />
             
             <ImageView
                 android:id="@+id/home_square_icon"
                 android:layout_width="59px"
                 android:layout_height="22px"
                 android:layout_gravity="left"
                 android:layout_marginLeft="242px"
                 android:layout_marginTop="287px"
                 android:background="@drawable/square_tag"
                 android:visibility="gone" />

	    </FrameLayout>
        
    </FrameLayout>

	<LinearLayout
        android:id="@+id/bottom_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_gravity="bottom"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical" >

		<LinearLayout
			android:id="@+id/bottom_menu_1"
			android:layout_width="480px"
			android:layout_height="74px"
			android:orientation="horizontal"   >
			<ImageView
					android:id="@+id/bottom_1"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/home_k_icon"
					android:layout_gravity="left"/>
			<ImageView
					android:id="@+id/bottom_2"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/audition_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_3"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/album_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_4"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/squre_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_5"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/more_k_icon"
					android:layout_gravity="left"/>
		</LinearLayout>
		
		<LinearLayout
			android:id="@+id/bottom_menu_2"
			android:layout_width="480px"
			android:layout_height="74px"
			android:orientation="horizontal"   >
			<ImageView
					android:id="@+id/bottom_6"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/search_k_icon"
					android:layout_gravity="left"/>
			<ImageView
					android:id="@+id/bottom_7"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/mypagehome_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_8"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/favoriteshome_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_9"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/settingshome_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_10"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/logohome_icon"
					android:layout_gravity="left"/>
		</LinearLayout>
	 </LinearLayout>
</FrameLayout>