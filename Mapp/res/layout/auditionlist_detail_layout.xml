<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    

    <FrameLayout 
        android:id="@+id/main_screen"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical"
	     >
         <FrameLayout 
	        android:id="@+id/title_bar"
		    android:layout_width="match_parent"
		    android:layout_height="66px"
		    android:orientation="vertical"
		    android:background="@drawable/title_bar_bg" >
		    
            <ImageView
                android:id="@+id/title_icon"
                android:layout_width="45px"
                android:layout_height="45px"
                android:layout_gravity="left"
                android:layout_marginLeft="31px"
                android:layout_marginTop="10px"
                android:background="@drawable/right_back_icon" />
            
            <TextView
		      	android:id="@+id/title_name"
		       	android:layout_width="match_parent"
		       	android:layout_height="wrap_content"
		       	android:layout_marginLeft="98px"
                android:layout_marginTop="10px"
		       	android:ellipsize="end"
		       	android:lines="1"
		       	android:maxLines="1"
		       	android:text="오디션"
		       	android:textColor="#FFFFFF"
		    	android:textSize="15dip" />
            
            <TextView
		      	android:id="@+id/title_desc"
		       	android:layout_width="match_parent"
		       	android:layout_height="wrap_content"
		       	android:layout_marginLeft="99px"
                android:layout_marginTop="34px"
		       	android:ellipsize="end"
		       	android:lines="1"
		       	android:maxLines="1"
		       	android:text="주간베스트"
		       	android:textColor="#FFFF00"
		    	android:textSize="15dip" />

            <ImageView
                android:id="@+id/title_popup"
                android:layout_width="45px"
                android:layout_height="45px"
                android:layout_gravity="left"
                android:layout_marginLeft="410px"
                android:layout_marginTop="10px"
                android:background="@drawable/right_share_icon" />
            
	    </FrameLayout>
	    
         
         <FrameLayout 
	        android:id="@+id/main_layout"
		    android:layout_width="match_parent"
		    android:layout_height="match_parent"    
		    android:layout_marginTop="68px"
		    android:orientation="vertical"
		    android:background="@drawable/bg" >
		    
			<ImageView
                 android:id="@+id/audition_detail_movie_back"
                 android:layout_width="465px"
                 android:layout_height="261px"
                 android:layout_gravity="left"
                 android:layout_marginLeft="7px"
                 android:layout_marginTop="7px"
                 android:background="@drawable/home_audition_vacuum_2" />
			
			<ImageView
                 android:id="@+id/audition_detail_movie_favorite"
                 android:layout_width="48px"
                 android:layout_height="66px"
                 android:layout_gravity="left"
                 android:layout_marginLeft="48px"
                 android:layout_marginTop="104px"
                 android:background="@drawable/audition_favorite_icon" />
			
			<ImageView
                 android:id="@+id/audition_detail_movie_play"
                 android:layout_width="126px"
                 android:layout_height="126px"
                 android:layout_gravity="left"
                 android:layout_marginLeft="177px"
                 android:layout_marginTop="85px"
                 android:background="@drawable/audition_play_bt_small" />
			
			
			<ImageView
                 android:id="@+id/audition_detail_movie_like"
                 android:layout_width="74px"
                 android:layout_height="68px"
                 android:layout_gravity="left"
                 android:layout_marginLeft="368px"
                 android:layout_marginTop="105px"
                 android:background="@drawable/audition_like_icon" />
			
			<TextView
		      	android:id="@+id/audition_detail_movie_like_count"
		       	android:layout_width="30dip"
		       	android:layout_height="wrap_content"
		       	android:ellipsize="end"
		       	android:lines="1"
		       	android:maxLines="1"
		       	android:text="1"
		       	android:layout_marginLeft="383px"
                android:layout_marginTop="115px"
		       	android:textColor="#000000"
		    	android:textSize="15dip"
		    	android:gravity="center" />
			
			<ListView
			    android:id="@+id/audition_info_listview"
			    
			    android:layout_height="386px"
			    android:layout_marginTop="280px"
			    android:background="#FFFFFF"
			    
			    android:layout_width="465px"
                android:layout_marginLeft="7px" />
			
			<ImageView
			    android:id="@+id/audition_detail_profile_back"
			    android:layout_width="match_parent"
			    android:layout_height="64px"
			    android:layout_gravity="left"
			    android:layout_marginTop="666px"
			    android:background="@drawable/title_bar_bg" />
			
			<ImageView
			    android:id="@+id/audition_detail_profile_img"
			    android:layout_width="56px"
			    android:layout_height="57px"
			    android:layout_gravity="left"
			    android:layout_marginLeft="20px"
			    android:layout_marginTop="670px"
			    android:background="@drawable/audition_profile_area" />
			
			
			<TextView
		      	android:id="@+id/audition_detail_profile_name"
		       	android:layout_width="350px"
		       	android:layout_height="wrap_content"
		       	android:layout_marginLeft="100px"
                android:layout_marginTop="686px"
		       	android:ellipsize="end"
		       	android:lines="1"
		       	android:maxLines="1"
		       	android:text="AT153"
		       	android:textColor="#FFFFFF"
		    	android:textSize="15dip" />
			
			
			<ImageView
			    android:id="@+id/audition_detail_profile_cur"
			    android:layout_width="15px"
			    android:layout_height="23px"
			    android:layout_gravity="left"
			    android:layout_marginLeft="431px"
			    android:layout_marginTop="686px"
			    android:background="@drawable/arrow_white" />

	    </FrameLayout>
        
    </FrameLayout>

    <LinearLayout
        android:id="@+id/bottom_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_gravity="bottom"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical" >

		<LinearLayout
			android:id="@+id/bottom_menu_1"
			android:layout_width="480px"
			android:layout_height="74px"
			android:orientation="horizontal"   >
			<ImageView
					android:id="@+id/bottom_1"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/home_k_icon"
					android:layout_gravity="left"/>
			<ImageView
					android:id="@+id/bottom_2"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/audition_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_3"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/album_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_4"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/squre_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_5"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/more_k_icon"
					android:layout_gravity="left"/>
		</LinearLayout>
		
		<LinearLayout
			android:id="@+id/bottom_menu_2"
			android:layout_width="480px"
			android:layout_height="74px"
			android:orientation="horizontal"   >
			<ImageView
					android:id="@+id/bottom_6"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/search_k_icon"
					android:layout_gravity="left"/>
			<ImageView
					android:id="@+id/bottom_7"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/mypagehome_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_8"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/favoriteshome_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_9"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/settingshome_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_10"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/logohome_icon"
					android:layout_gravity="left"/>
		</LinearLayout>
	 </LinearLayout>
</RelativeLayout>