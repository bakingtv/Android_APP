<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    
    
		    

    <FrameLayout 
        android:id="@+id/main_screen"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical"
	     >
		    
         <FrameLayout 
	        android:id="@+id/title_bar"
		    android:layout_width="match_parent"
		    android:layout_height="66px"
		    android:orientation="vertical"
		    android:background="@drawable/title_bar_bg" >
		    
            <ImageView
                android:id="@+id/title_icon"
                android:layout_width="45px"
                android:layout_height="45px"
                android:layout_gravity="left"
                android:layout_marginLeft="31px"
                android:layout_marginTop="10px"
                android:background="@drawable/right_back_icon" />

            
            <TextView
		      	android:id="@+id/title_name"
		       	android:layout_width="wrap_content"
		       	android:layout_height="wrap_content"
		       	android:layout_marginLeft="98px"
		       	android:layout_marginRight="10px"
                android:layout_marginTop="15px"
		       	android:ellipsize="end"
		       	android:lines="1"
		       	android:maxLines="1"
		       	android:text="스퀘어"
		       	android:textColor="#FFFFFF"
		    	android:textSize="25dip" />
            
             <TextView
		      	android:id="@+id/title_desc"
		       	android:layout_width="wrap_content"
		       	android:layout_height="wrap_content"
		       	android:layout_marginLeft="98px"
		       	android:layout_marginRight="10px"
                android:layout_marginTop="10px"
		       	android:ellipsize="end"
		       	android:lines="1"
		       	android:maxLines="1"
		       	android:text=""
		       	android:textColor="#FFFFFF"
		    	android:textSize="0dip" />


	    </FrameLayout>
	    
         		  
		<ScrollView
			android:id="@+id/main_layout"
		    android:layout_width="match_parent"
		    android:layout_height="match_parent"    
		    android:layout_marginTop="68px"
		    android:orientation="vertical"
		    android:background="#FFFFFF">
            <LinearLayout
		        android:id="@+id/detail_desc"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:orientation="vertical" >
			 	<com.euiweonjeong.base.view.GalleryForOneFling
	                 android:id="@+id/square_detail_img"
	                 android:layout_width="480px"
	                 android:layout_height="261px"
	                 android:layout_gravity="left"
	                 android:background="@drawable/home_audition_vacuum" />
			 	
			 	<LinearLayout
			        android:id="@+id/detail_buttons"
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:orientation="horizontal" 
			        android:layout_marginTop="-20px"
			        android:gravity="center">
			        
			 	    <ImageView
		                android:id="@+id/square_btn_1"
		                android:layout_width="9px"
		                android:layout_height="9px"
		                android:layout_marginRight="7px"
		                android:layout_gravity="center"
		                android:background="@drawable/home_control" />
			 	    
			 	    <ImageView
		                android:id="@+id/square_btn_1_1"
		                android:layout_width="9px"
		                android:layout_height="9px"
		                android:layout_marginLeft="-16px"
		                android:layout_marginRight="7px"
		                android:layout_gravity="center"
		                android:background="@drawable/home_control_selected" />
			 	    
			 	    <ImageView
		                android:id="@+id/square_btn_2"
		                android:layout_width="9px"
		                android:layout_height="9px"
		                android:layout_marginRight="7px"
		                android:layout_gravity="center"
		                android:background="@drawable/home_control" />
			 	    
			 	    <ImageView
		                android:id="@+id/square_btn_2_1"
		                android:layout_width="9px"
		                android:layout_height="9px"
		                android:layout_marginLeft="-16px"
		                android:layout_marginRight="7px"
		                android:layout_gravity="center"
		                android:background="@drawable/home_control_selected" />
			 	    
			 	    <ImageView
		                android:id="@+id/square_btn_3"
		                android:layout_width="9px"
		                android:layout_height="9px"
		                android:layout_marginRight="7px"
		                android:layout_gravity="center"
		                android:background="@drawable/home_control" />
			 	    
			 	    <ImageView
		                android:id="@+id/square_btn_3_1"
		                android:layout_width="9px"
		                android:layout_height="9px"
		                android:layout_marginLeft="-16px"
		                android:layout_marginRight="7px"
		                android:layout_gravity="center"
		                android:background="@drawable/home_control_selected" />
			 	    
			 	    
			 	    <ImageView
		                android:id="@+id/square_btn_4"
		                android:layout_width="9px"
		                android:layout_height="9px"
		                android:layout_marginRight="7px"
		                android:layout_gravity="center"
		                android:background="@drawable/home_control" />
			 	    
			 	    <ImageView
		                android:id="@+id/square_btn_4_1"
		                android:layout_width="9px"
		                android:layout_height="9px"
		                android:layout_marginLeft="-16px"
		                android:layout_marginRight="7px"
		                android:layout_gravity="center"
		                android:background="@drawable/home_control_selected" />
			 	    
			 	    <ImageView
		                android:id="@+id/square_btn_5"
		                android:layout_width="9px"
		                android:layout_height="9px"
		                android:layout_marginRight="7px"
		                android:layout_gravity="center"
		                android:background="@drawable/home_control" />
			 	    
			 	    <ImageView
		                android:id="@+id/square_btn_5_1"
		                android:layout_width="9px"
		                android:layout_height="9px"
		                android:layout_marginLeft="-16px"
		                android:layout_marginRight="7px"
		                android:layout_gravity="center"
		                android:background="@drawable/home_control_selected" />
			 	    
			 	    
			 	    <ImageView
		                android:id="@+id/square_btn_6"
		                android:layout_width="9px"
		                android:layout_height="9px"
		                android:layout_marginRight="7px"
		                android:layout_gravity="center"
		                android:background="@drawable/home_control" />
			 	    
			 	    <ImageView
		                android:id="@+id/square_btn_6_1"
		                android:layout_width="9px"
		                android:layout_height="9px"
		                android:layout_marginLeft="-16px"
		                android:layout_marginRight="7px"
		                android:layout_gravity="center"
		                android:background="@drawable/home_control_selected" />
			 	    
			 	    
			 	    <ImageView
		                android:id="@+id/square_btn_7"
		                android:layout_width="9px"
		                android:layout_height="9px"
		                android:layout_marginRight="7px"
		                android:layout_gravity="center"
		                android:background="@drawable/home_control" />
			 	    
			 	    <ImageView
		                android:id="@+id/square_btn_7_1"
		                android:layout_width="9px"
		                android:layout_height="9px"
		                android:layout_marginLeft="-16px"
		                android:layout_marginRight="7px"
		                android:layout_gravity="center"
		                android:background="@drawable/home_control_selected" />
			 	
			 	</LinearLayout>
			 	
			 	<LinearLayout
			        android:id="@+id/detail_text_layout"
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:orientation="vertical" 
			        android:background="#FFFFFF"
			        >
			 	    
			 	    <TextView
				      	android:id="@+id/square_text_view"
				       	android:layout_width="match_parent"
				       	android:layout_height="wrap_content"
				       	android:layout_marginLeft="10px"
				       	android:layout_marginRight="10px"
		                android:layout_marginTop="10px"
				       	android:ellipsize="end"
				       	android:text="스퀘어"
				       	android:textColor="#000000"
				    	android:textSize="15dip" 
				    	android:autoLink="web"/>
			 	    
			 	</LinearLayout>
			 </LinearLayout>
	    </ScrollView>
        
    </FrameLayout>

    <LinearLayout
        android:id="@+id/bottom_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_gravity="bottom"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical" >

		<LinearLayout
			android:id="@+id/bottom_menu_1"
			android:layout_width="480px"
			android:layout_height="74px"
			android:orientation="horizontal"   >
			<ImageView
					android:id="@+id/bottom_1"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/home_k_icon"
					android:layout_gravity="left"/>
			<ImageView
					android:id="@+id/bottom_2"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/audition_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_3"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/album_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_4"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/squre_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_5"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/more_k_icon"
					android:layout_gravity="left"/>
		</LinearLayout>
		
		<LinearLayout
			android:id="@+id/bottom_menu_2"
			android:layout_width="480px"
			android:layout_height="74px"
			android:orientation="horizontal"   >
			<ImageView
					android:id="@+id/bottom_6"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/search_k_icon"
					android:layout_gravity="left"/>
			<ImageView
					android:id="@+id/bottom_7"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/mypagehome_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_8"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/favoriteshome_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_9"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/settingshome_k_icon"
					android:layout_gravity="left"/>
			
			<ImageView
					android:id="@+id/bottom_10"
					android:layout_width="96px"
					android:layout_height="74px"
					android:background="@drawable/logohome_icon"
					android:layout_gravity="left"/>
		</LinearLayout>
	 </LinearLayout>
</RelativeLayout>