      	<activity android:name=".UploadActivity" android:screenOrientation="portrait"  android:windowSoftInputMode="stateHidden"/> 
      	<activity android:name=".UploadDirectActivity" android:screenOrientation="portrait"  android:windowSoftInputMode="stateHidden"/> 
      	<activity android:name=".MappActivity" android:screenOrientation="portrait"  android:windowSoftInputMode="stateHidden"/> 
      	
      	
      	<meta-data android:value="@string/app_id" android:name="com.facebook.sdk.ApplicationId"/>
      	<activity android:name="com.facebook.LoginActivity" android:screenOrientation="portrait"></activity>
      	
      	
      	<activity
             android:configChanges="orientation|keyboardHidden"
             android:label="Twitter Login"
             android:name=".TwitterLogin"
             android:screenOrientation="portrait"
             android:theme="@android:style/Theme.NoTitleBar"
             android:windowSoftInputMode="stateHidden" >
             <intent-filter >
                 <action android:name="com.android.twittercon.TWITTER_LOGIN" />
 
                 <category android:name="android.intent.category.DEFAULT" />
                 <category android:name="android.intent.category.BROWSABLE" />
             </intent-filter>
         </activity>
      	
      	  
        
      	 
