      	<activity android:name=".SquareWebActivity" android:screenOrientation="portrait" />
      	<activity android:name=".SquareEventDetailActivity" android:screenOrientation="portrait" />
      	<activity android:name=".SquareNetworkDetailActivity" android:screenOrientation="portrait" />
      	<activity android:name=".AlbumDetailActivity" android:screenOrientation="portrait" />
      	<activity android:name=".AlbumDetailTrackActivity" android:screenOrientation="portrait" /> 
