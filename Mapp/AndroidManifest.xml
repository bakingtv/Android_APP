      	<activity android:name=".SettingLoginActivity" android:screenOrientation="portrait" android:windowSoftInputMode="stateHidden"/>
      	<activity android:name=".SettingAppInfoActivity" android:screenOrientation="portrait" android:windowSoftInputMode="stateHidden"/>
      	<activity android:name=".SettingPasswordActivity" android:screenOrientation="portrait" android:windowSoftInputMode="stateHidden"/>
